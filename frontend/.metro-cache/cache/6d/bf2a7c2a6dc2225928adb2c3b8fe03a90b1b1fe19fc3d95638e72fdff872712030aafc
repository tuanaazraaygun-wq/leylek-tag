artTag = async () => {\n      if (!activeTag) return;\n      try {\n        const response = await fetch(`${API_URL}/driver/start-tag/${activeTag.id}?user_id=${user.id}`, {\n          method: 'POST'\n        });\n        const data = await response.json();\n        if (data.success) {\n          Alert.default.alert('Başarılı', 'Yolculuk başlatıldı');\n          loadActiveTag();\n        }\n      } catch (error) {\n        Alert.default.alert('Hata', 'Yolculuk başlatılamadı');\n      }\n    };\n    const handleCompleteTag = async () => {\n      if (!activeTag) return;\n      Alert.default.alert('Yolculuğu Tamamla', 'Yolculuk tamamlandı olarak işaretlenecek', [{\n        text: 'İptal',\n        style: 'cancel'\n      }, {\n        text: 'Tamamla',\n        onPress: async () => {\n          try {\n            const response = await fetch(`${API_URL}/driver/complete-tag/${activeTag.id}?user_id=${user.id}`, {\n              method: 'POST'\n            });\n            const data = await response.json();\n            if (data.success) {\n              Alert.default.alert('Başarılı', 'Yolculuk tamamlandı');\n              setActiveTag(null);\n              loadRequests();\n            }\n          } catch (error) {\n            Alert.default.alert('Hata', 'İşlem başarısız');\n          }\n        }\n      }]);\n    };\n    // SESLİ ARAMA - Driver için mock fonksiyon\n    const handleDriverVoiceCall = () => {\n      setCalling(true);\n      Alert.default.alert('📞 Sesli Arama', 'Yolcunuzla bağlantı kuruluyor...\\n\\n🔒 Uçtan uca şifreli arama\\n📱 Gerçek numaralar gizli', [{\n        text: 'Aramayı Sonlandır',\n        onPress: () => {\n          setCalling(false);\n          Alert.default.alert('📞 Arama Sonlandırıldı', 'Arama başarıyla sonlandırıldı.');\n        }\n      }]);\n    };\n    return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n      style: styles.container,\n      children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)