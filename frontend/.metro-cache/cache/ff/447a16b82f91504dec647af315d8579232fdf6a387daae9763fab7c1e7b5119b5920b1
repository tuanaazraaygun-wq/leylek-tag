) {\n        Alert.default.alert('Hata', 'Çağrı gönderilemedi');\n      } finally {\n        setLoading(false);\n      }\n    };\n\n    // SESLİ ARAMA - Mock fonksiyon\n    const handleVoiceCall = () => {\n      setCalling(true);\n      Alert.default.alert('📞 Sesli Arama', 'Sürücünüzle bağlantı kuruluyor...\\n\\n🔒 Uçtan uca şifreli arama\\n📱 Gerçek numaralar gizli', [{\n        text: 'Aramayı Sonlandır',\n        onPress: () => {\n          setCalling(false);\n          Alert.default.alert('📞 Arama Sonlandırıldı', 'Arama başarıyla sonlandırıldı.');\n        }\n      }]);\n    };\n    const handleAcceptOffer = async offerId => {\n      if (!activeTag) return;\n      const selectedOffer = offers.find(o => o.id === offerId);\n      if (!selectedOffer) return;\n      try {\n        const response = await fetch(`${API_URL}/passenger/accept-offer?user_id=${user.id}`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            tag_id: activeTag.id,\n            offer_id: offerId\n          })\n        });\n        const data = await response.json();\n        if (data.success) {\n          // Sadece sürücü adını kaydet, arama başlatma\n          setSelectedDriverName(selectedOffer.driver_name);\n          loadActiveTag();\n          Alert.default.alert('Başarılı', 'Eşleşme sağlandı! Haritada konumları görebilirsiniz.');\n        }\n      } catch (error) {\n        Alert.default.alert('Hata', 'Teklif kabul edilemedi');\n      }\n    };\n    const handleCancelTag = async () => {\n      if (!activeTag) return;\n      Alert.default.alert('Çağrıyı İptal Et', 'Çağrınızı iptal etmek istediğinizden emin misiniz? Sürücülere bildirim gönderilecek.', [{\n        text: 'Vazgeç',\n        style: 'cancel'\n      }, {\n        text: 'İptal Et',\n        style: 'destructive',\n        onPress: async () => {\n          try {\n            const response = await fetch(`${API_URL}/passenger/cancel-tag?user_id=${user.id}`, {\n              method: 'POST',\n              headers: {\n                'Content-Type': 'application/json'\n              },\n              body: JSON.stringify({\n                tag_id: activeTag.id\n              })\n            });\n            const data = await response.json();\n            if (data.success) {\n              Alert.default.alert('✅ İptal Edildi', 'Çağrınız başarıyla iptal edildi.');\n              setActiveTag(null);\n              setOffers([]);\n            } else {\n              Alert.default.alert('Hata', data.detail || 'Çağrı iptal edilemedi');\n            }\n          } catch (error) {\n            Alert.default.alert('Hata', 'Çağrı iptal edilemedi');\n          }\n        }\n      }]);\n    };\n    const handleDestinationSelect = async (address, lat, lng) => {\n      const newDestination = {\n        address,\n        latitude: lat,\n        longitude: lng\n      };\n      setDestination(newDestination);\n      setShowDestinationPicker(false);\n\n      // Eğer aktif TAG varsa, hedefi güncelle\n      if (activeTag) {\n        try {\n          const response = await fetch(`${API_URL}/passenger/update-destination?user_id=${user.id}`, {\n            method: 'POST',\n            headers: {\n              'Content-Type': 'application/json'\n            },\n            body: JSON.stringify({\n              tag_id: activeTag.id,\n              dropoff_location: address,\n              dropoff_lat: lat,\n              dropoff_lng: lng\n            })\n          });\n          const data = await response.json();\n          if (data.success) {\n            Alert.default.alert('✅ Güncellendi', 'Hedef adresiniz başarıyla güncellendi.');\n            loadActiveTag(); // TAG'i yeniden yükle\n          } else {\n            Alert.default.alert('Hata', data.detail || 'Hedef güncellenemedi');\n          }\n        } catch (error) {\n          Alert.default.alert('Hata', 'Hedef güncellenemedi');\n        }\n      }\n    };