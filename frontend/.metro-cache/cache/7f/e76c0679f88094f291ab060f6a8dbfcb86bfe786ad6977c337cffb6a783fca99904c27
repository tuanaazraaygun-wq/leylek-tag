\n    setScreen\n  }) {\n    const [activeTag, setActiveTag] = (0, _react.useState)(null);\n    const [requests, setRequests] = (0, _react.useState)([]);\n    const [loading, setLoading] = (0, _react.useState)(false);\n    const [calling, setCalling] = (0, _react.useState)(false);\n\n    // GPS & Map states\n    const [userLocation, setUserLocation] = (0, _react.useState)(null);\n    const [passengerLocation, setPassengerLocation] = (0, _react.useState)(null);\n\n    // Mesafe ve sÃ¼re state'leri\n    const [realDistance, setRealDistance] = (0, _react.useState)(0);\n    const [estimatedTime, setEstimatedTime] = (0, _react.useState)(0);\n\n    // ==================== DAILY.CO ARAMA SISTEMI - SOFOR ====================\n    const [dailyCallActive, setDailyCallActive] = (0, _react.useState)(false);\n    const [dailyRoomUrl, setDailyRoomUrl] = (0, _react.useState)(null);\n    const [dailyCallType, setDailyCallType] = (0, _react.useState)('audio');\n    const [dailyCallerName, setDailyCallerName] = (0, _react.useState)('');\n    const [dailyCallerId, setDailyCallerId] = (0, _react.useState)('');\n    const [dailyReceiverId, setDailyReceiverId] = (0, _react.useState)(''); // ðŸ†• Added for proper termination\n    const [incomingDailyCall, setIncomingDailyCall] = (0, _react.useState)(false);\n    const [dailyRoomName, setDailyRoomName] = (0, _react.useState)('');\n    const [incomingCallTagId, setIncomingCallTagId] = (0, _react.useState)(''); // ðŸ†• Gelen arama tag_id\n\n    // Giden Arama State (Araniyor...) - SOFOR\n    const [outgoingCall, setOutgoingCall] = (0, _react.useState)(false);\n    const [outgoingCallData, setOutgoingCallData] = (0, _react.useState)(null);\n\n    // ðŸ†• Chat State'leri (SÃ¼rÃ¼cÃ¼)\n    const [driverChatVisible, setDriverChatVisible] = (0, _react.useState)(false);\n    const [driverChatMessages, setDriverChatMessages] = (0, _react.useState)([]);\n\n    // Eski Agora state'leri (artik kullanilmiyor ama kaldirilmadi)\n    const [showCallScreen, setShowCallScreen] = (0, _react.useState)(false);\n    const [callScreenData, setCallScreenData] = (0, _react.useState)(null);\n\n    // Socket.IO arama durumlari\n    const [callAccepted, setCallAccepted] = (0, _react.useState)(false);\n    const [callRejected, setCallRejected] = (0, _react.useState)(false);\n    const [callEnded, setCallEnded] = (0, _react.useState)(false);\n    const [receiverOffline, setReceiverOffline] = (0, _react.useState)(false);\n\n    // Arama kilidi\n    const isCallActiveRef = (0, _react.useRef)(false);\n\n    // ==================== SOCKET.IO HOOK - ÅžOFÃ–R ====================\n    const {\n      socket: driverSocket,\n      // ðŸ†• Socket instance\n      isConnected: socketConnected,\n      isRegistered: socketRegistered,\n      startCall: socketStartCall,\n      acceptCall: socketAcceptCall,\n      rejectCall: socketRejectCall,\n      endCall: socketEndCall,\n      // TAG & Teklif iÃ§in yeni fonksiyonlar\n      emitSendOffer: socketSendOffer,\n      emitDriverLocationUpdate,\n      // ðŸ†• YENÄ°: ÅžofÃ¶r konum gÃ¼ncelleme (RAM)\n      // ðŸ†• Daily.co Call Signaling\n      emitCallInvite,\n      emitCallAccepted,\n      emitCallRejected,\n      // ðŸ†• YENÄ°: Sync Call Events\n      emitCallAccept,\n      emitCallReject,\n      emitCallCancel,\n      emitCallEnd,\n      acceptDailyCall,\n      rejectDailyCall,\n      endDailyCall,\n      // ðŸ†• MesajlaÅŸma\n      emitSendMessage: driverEmitSendMessage\n    } = (0, useSocket.default)({\n      userId: user?.id || null,\n      userRole: 'driver',\n      // ðŸ†• Daily.co Gelen Arama - ÅžOFÃ–R (VÄ°BRASYON + Accept/Reject)\n      onIncomingDailyCall: data => {\n        console.log('ðŸ“¹ ÅžOFÃ–R - DAILY.CO GELEN ARAMA:', data);\n        // Gelen arama state'lerini ayarla - room henÃ¼z yok\n        setDailyCallerId(data.caller_id);\n        setDailyCallerName(data.caller_name || 'Yolcu');\n        setDailyCallType(data.call_type || 'audio');\n        setIncomingDailyCall(true);\n        // tagId'yi de sakla\n        setIncomingCallTagId(data.tag_id || '');\n      },\n      // ðŸ†• YENÄ°: call_accepted - HER Ä°KÄ° TARAFA aynÄ± anda geliyor!\n      onCallAcceptedNew: data => {\n        console.log('âœ… ÅžOFÃ–R - CALL_ACCEPTED (SYNC) - Daily odasÄ± hazÄ±r:', data);\n        // Her iki taraf da bu eventi alÄ±yor - Daily.co'ya gir\n        setDailyRoomUrl(data.room_url);\n        setDailyRoomName(data.room_name);\n        setDailyCallType(data.call_type);\n        // CRITICAL: Store caller/receiver IDs for proper termination\n        setDailyCallerId(data.caller_id);\n        setDailyReceiverId(data.receiver_id);\n        // Arayan mÄ± aranan mÄ±?\n        const isCaller = user?.id === data.caller_id;\n        setDailyCallerName(isCaller ? 'Yolcu' : dailyCallerName || 'Yolcu');\n        setDailyCallActive(true);\n        // Reset states - navigation YOK\n        setOutgoingCall(false);\n        setOutgoingCallData(null);\n        setIncomingDailyCall(false);\n      },\n      onDailyCallAccepted: data => {\n        console.log('SOFOR - DAILY.CO ARAMA KABUL EDILDI (ESKÄ°):', data);\n        // Eski event - artÄ±k onCallAcceptedNew kullanÄ±lÄ±yor\n      },\n      onDailyCallRejected: data => {\n        console.log('SOFOR - DAILY.CO ARAMA REDDEDILDI:', data);\n        setOutgoingCall(false);\n        setOutgoingCallData(null);\n        setDailyCallActive(false);\n        setIncomingDailyCall(false);\n        setDailyRoomUrl(null);\n        setDailyCallerId('');\n        setDailyReceiverId('');\n        Alert.default.alert('Bilgi', 'Arama reddedildi');\n      },\n      // ðŸ†• YENÄ°: call_cancelled - Arayan iptal etti\n      onCallCancelled: data => {\n        console.log('ðŸš« ÅžOFÃ–R - ARAMA Ä°PTAL EDÄ°LDÄ°:', data);\n        setIncomingDailyCall(false);\n      },\n      // ðŸ†• YENÄ°: call_ended - GÃ¶rÃ¼ÅŸme bitti\n      onCallEndedNew: data => {\n        console.log('ðŸ“´ ÅžOFÃ–R - CALL_ENDED:', data);\n        setDailyCallActive(false);\n        setDailyR