leme yok)\n      fetch(`${API_URL}/driver/send-offer?user_id=${user.id}`, {\n        method: 'POST',\n        headers: {\n          'Content-Type': 'application/json'\n        },\n        body: JSON.stringify({\n          tag_id: tagId,\n          price: price,\n          latitude: userLocation?.latitude || 0,\n          longitude: userLocation?.longitude || 0\n        })\n      }).then(r => r.json()).then(data => {\n        setOfferSending(false);\n        if (data.success || data.offer_id) {\n          console.log('âœ… Teklif Supabase\\'e kaydedildi');\n        } else {\n          console.log('âš ï¸ Supabase kayÄ±t hatasÄ±:', data.detail);\n          // Hata olursa geri ekle (opsiyonel)\n        }\n      }).catch(err => {\n        setOfferSending(false);\n        console.log('âš ï¸ REST API hatasÄ± (socket zaten gÃ¶nderdi):', err);\n      });\n    };\n    const handleStartTag = async () => {\n      if (!activeTag) return;\n      try {\n        const response = await fetch(`${API_URL}/driver/start-tag/${activeTag.id}?user_id=${user.id}`, {\n          method: 'POST'\n        });\n        const data = await response.json();\n        if (data.success) {\n          loadActiveTag();\n        }\n      } catch (error) {\n        Alert.default.alert('Hata', 'Yolculuk baÅŸlatÄ±lamadÄ±');\n      }\n    };\n    const handleCompleteTag = async () => {\n      if (!activeTag) return;\n      Alert.default.alert('YolculuÄŸu Tamamla', 'Yolculuk tamamlandÄ± olarak iÅŸaretlenecek', [{\n        text: 'Ä°ptal',\n        style: 'cancel'\n      }, {\n        text: 'Tamamla',\n        onPress: async () => {\n          try {\n            const response = await fetch(`${API_URL}/driver/complete-tag/${activeTag.id}?user_id=${user.id}`, {\n              method: 'POST'\n            });\n            const data = await response.json();\n            if (data.success) {\n              setActiveTag(null);\n              loadRequests();\n            }\n          } catch (error) {\n            Alert.