error);\n        return 'device_' + Date.now();\n      }\n    };\n    (0, _react.useEffect)(() => {\n      initializeApp();\n    }, []);\n    const initializeApp = async () => {\n      // Ã–nce cihaz ID'yi al\n      const dId = await getOrCreateDeviceId();\n      setDeviceId(dId);\n\n      // KVKK onayÄ± kontrolÃ¼ (telefon numarasÄ±na gÃ¶re)\n      const storedKVKKPhone = await AsyncStorage.default.getItem('kvkk_accepted_phone');\n      // Onay varsa set et\n      if (storedKVKKPhone) {\n        setKvkkAccepted(true);\n      }\n\n      // Sonra kullanÄ±cÄ±yÄ± yÃ¼kle\n      await loadUser();\n    };\n\n    // KVKK onayÄ±nÄ± kaydet (telefon numarasÄ±na gÃ¶re)\n    const saveKVKKConsent = async phoneNumber => {\n      try {\n        await AsyncStorage.default.setItem('kvkk_accepted_phone', phoneNumber);\n        setKvkkAccepted(true);\n        console.log('âœ… KVKK onayÄ± kaydedildi:', phoneNumber);\n      } catch (error) {\n        console.error('KVKK kayÄ±t hatasÄ±:', error);\n      }\n    };\n\n    // KVKK onayÄ± kontrol (telefon deÄŸiÅŸtiÄŸinde yeniden iste)\n    const checkKVKKConsent = async phoneNumber => {\n      try {\n        const storedPhone = await AsyncStorage.default.getItem('kvkk_accepted_phone');\n        return storedPhone === phoneNumber;\n      } catch (error) {\n        return false;\n      }\n    };\n    (0, _react.useEffect)(() => {\n      if (user && screen === 'dashboard') {\n        requestLocationPermission().then(granted => {\n          if (granted) {\n            updateUserLocation();\n            // Her 0.5 saniyede bir konum gÃ¼ncelle - CANLI TAKÄ°P\n            const interval = setInterval(updateUserLocation, 500);\n            return () => clearInterval(interval);\n          }\n        });\n      }\n    }, [user, screen]);\n    const loadUser = async () => {\n      try {\n        const userData = await AsyncStorage.default.getItem('user');\n        const legalAcceptedStorage = await AsyncStorage.default.getItem('legal_accepted');\n        if (userData) {\n          const parsedUser = JSON.parse(userData);\n          setUser(parsedUser);\n\n          // Admin kontrolÃ¼ - 5326497412 ana admin\n          const cleanPhone = parsedUser.phone?.replace(/\\D/g, '');\n          const isMainAdmin = cleanPhone === '5326497412' || cleanPhone === '05326497412';\n          if (isMainAdmin) {\n            setIsAdmin(true);\n            // Admin direkt admin paneline gitsin\n            setShowAdminPanel(true);\n            setScreen('role-select');\n          } else {\n            // API'den admin kontrolÃ¼\n            try {\n              const res = await fetch(`${API_URL}/admin/check?phone=${cleanPhone}`);\n              const data = await res.json();\n              if (data.success && data.is_admin) {\n                setIsAdmin(true);\n                setShowAdminPanel(true); // DiÄŸer adminler de direkt panele\n              }\n            } catch (e) {}\n            setScreen('role-select');\n          }\n\n          // Legal consent kontrolÃ¼\n          if (legalAcceptedStorage !== 'true') {\n            setShowLegalConsent(true);\n          } else {\n            setLegalAccepted(true);\n          }\n        }\n      } catch (error) {\n        console.error('KullanÄ±cÄ± yÃ¼klenemedi:', error);\n      } finally {\n        setLoading(false);\n      }\n    };\n\n    // Legal consent kabul\n    const handleLegalAccept = async () => {\n      await AsyncStorage.default.setItem('legal_accepted', 'true');\n      setLegalAccepted(true);\n      setShowLegalConsent(false);\n    };\n\n    // Legal consent red\n    const handleLegalDecline = async () => {\n      Alert.default.alert('UyarÄ±', 'KullanÄ±m ÅŸartlarÄ±nÄ± kabul etmeden devam edemezsiniz.', [{\n        text: 'Tamam'\n      }]);\n    };\n    const saveUser = async userData => {\n      await AsyncStorage.default.setItem('user', JSON.stringify(userData));\n      setUser(userData);\n    };\n    const logout = async () => {\n      // Logout sÄ±rasÄ±nda push tok