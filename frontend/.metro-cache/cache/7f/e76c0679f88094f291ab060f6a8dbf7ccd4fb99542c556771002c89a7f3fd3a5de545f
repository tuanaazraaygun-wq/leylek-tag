      style: [cloudStyles.cloud, cloudStyles.cloud8, {\n          transform: [{\n            translateX: cloud8X\n          }]\n        }],\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_expoVectorIcons.Ionicons, {\n          name: \"cloud\",\n          size: 45,\n          color: \"rgba(63, 169, 245, 0.14)\"\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 112,\n          columnNumber: 9\n        }, this)\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 111,\n        columnNumber: 7\n      }, this)]\n    }, void 0, true, {\n      fileName: _jsxFileName,\n      lineNumber: 87,\n      columnNumber: 5\n    }, this);\n  };\n  const cloudStyles = StyleSheet.default.create({\n    container: {\n      position: 'absolute',\n      top: 0,\n      left: 0,\n      right: 0,\n      bottom: 0,\n      zIndex: 0\n    },\n    cloud: {\n      position: 'absolute'\n    },\n    cloud1: {\n      top: '5%'\n    },\n    cloud2: {\n      top: '12%'\n    },\n    cloud5: {\n      top: '18%'\n    },\n    cloud6: {\n      top: '8%',\n      left: '30%'\n    },\n    cloud3: {\n      top: '35%'\n    },\n    cloud4: {\n      top: '50%'\n    },\n    cloud7: {\n      top: '60%'\n    },\n    cloud8: {\n      top: '70%'\n    }\n  });\n\n  // Mesafe Hesaplama Fonksiyonu (Haversine)\n  const calculateDistance = (lat1, lon1, lat2, lon2) => {\n    const R = 6371; // DÃ¼nya'nÄ±n yarÄ±Ã§apÄ± (km)\n    const dLat = (lat2 - lat1) * Math.PI / 180;\n    const dLon = (lon2 - lon1) * Math.PI / 180;\n    const a = Math.sin(dLat / 2) * Math.sin(dLat / 2) + Math.cos(lat1 * Math.PI / 180) * Math.cos(lat2 * Math.PI / 180) * Math.sin(dLon / 2) * Math.sin(dLon / 2);\n    const c = 2 * Math.atan2(Math.sqrt(a), Math.sqrt(1 - a));\n    const distance = R * c;\n    return Math.round(distance * 10) / 10; // 1 ondalÄ±k basamak\n  };\n\n  // Leylek TAG Colors\n  const COLORS = {\n    primary: '#3FA9F5',\n    secondary: '#FF6B35',\n    background: '#FFFFFF',\n    surface: '#E9EDF2',\n    text: '#1B1B1E',\n    gray400: '#ADB5BD',\n    gray500: '#6C757D',\n    gray600: '#495057',\n    success: '#00C853',\n    info: '#007AFF'\n  };\n\n  // User Context\n\n  function App() {\n    const [user, setUser] = (0, _react.useState)(null);\n    const [loading, setLoading] = (0, _react.useState)(true);\n    const [screen, setScreen] = (0, _react.useState)('login');\n\n    // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n    // PERMISSION GATE - All permissions requested ONCE at app start\n    // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n    const [permissionsGranted, setPermissionsGranted] = (0, _react.useState)(false);\n    const [permissionChecking, setPermissionChecking] = (0, _react.useState)(true);\n    const [microphonePermission, setMicrophonePermission] = (0, _react.useState)(false);\n    const [cameraPermission, setCameraPermission] = (0, _react.useState)(false);\n\n    // Splash Screen\n    const [showSplash, setShowSplash] = (0, _react.useState)(true);\n\n    // KVKK OnayÄ±\n    const [kvkkAccepted, setKvkkAccepted] = (0, _react.useState)(false);\n    const [showKVKKModal, setShowKVKKModal] = (0, _react.useState)(false);\n    const [showSupportModal, setShowSupportModal] = (0, _react.useState)(false);\n\n    // Auth states\n    const [phone, setPhone] = (0, _react.useState)('');\n    const [otp, setOtp] = (0, _react.useState)('');\n    const [name, setName] = (0, _react.useState)('');\n    const [selectedCity, setSelectedCity] = (0, _react.useState)('');\n    const [cities, setCities] = (0, _react.useState)([]);\n    const [showCityPicker, setShowCityPicker] = (0, _react.useState)(false);\n\n    // Yeni Auth states\n    const [firstName, setFirstName] = (0, _react.useState)('');\n    const [l