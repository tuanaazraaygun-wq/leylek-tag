updateUserLocation();\n            // Her 10 saniyede bir konum g√ºncelle\n            const interval = setInterval(updateUserLocation, 10000);\n            return () => clearInterval(interval);\n          }\n        });\n      }\n    }, [user, screen]);\n    const loadUser = async () => {\n      try {\n        const userData = await AsyncStorage.default.getItem('user');\n        const legalAcceptedStorage = await AsyncStorage.default.getItem('legal_accepted');\n        if (userData) {\n          const parsedUser = JSON.parse(userData);\n          setUser(parsedUser);\n\n          // Admin kontrol√º - 5326497412 ana admin\n          const cleanPhone = parsedUser.phone?.replace(/\\D/g, '');\n          const isMainAdmin = cleanPhone === '5326497412' || cleanPhone === '05326497412';\n          if (isMainAdmin) {\n            setIsAdmin(true);\n            // Admin direkt admin paneline gitsin\n            setShowAdminPanel(true);\n            setScreen('role-select');\n          } else {\n            // API'den admin kontrol√º\n            try {\n              const res = await fetch(`${API_URL}/admin/check?phone=${cleanPhone}`);\n              const data = await res.json();\n              if (data.success && data.is_admin) {\n                setIsAdmin(true);\n                setShowAdminPanel(true); // Diƒüer adminler de direkt panele\n              }\n            } catch (e) {}\n            setScreen('role-select');\n          }\n\n          // Legal consent kontrol√º\n          if (legalAcceptedStorage !== 'true') {\n            setShowLegalConsent(true);\n          } else {\n            setLegalAccepted(true);\n          }\n        }\n      } catch (error) {\n        console.error('Kullanƒ±cƒ± y√ºklenemedi:', error);\n      } finally {\n        setLoading(false);\n      }\n    };\n\n    // Legal consent kabul\n    const handleLegalAccept = async () => {\n      await AsyncStorage.default.setItem('legal_accepted', 'true');\n      setLegalAccepted(true);\n      setShowLegalConsent(false);\n    };\n\n    // Legal consent red\n    const handleLegalDecline = async () => {\n      Alert.default.alert('Uyarƒ±', 'Kullanƒ±m ≈üartlarƒ±nƒ± kabul etmeden devam edemezsiniz.', [{\n        text: 'Tamam'\n      }]);\n    };\n    const saveUser = async userData => {\n      await AsyncStorage.default.setItem('user', JSON.stringify(userData));\n      setUser(userData);\n    };\n    const logout = async () => {\n      // Logout sƒ±rasƒ±nda push token'ƒ± sil\n      if (user?.id) {\n        await removePushToken(user.id);\n      }\n      await AsyncStorage.default.removeItem('user');\n      setUser(null);\n      setScreen('login');\n      setPhone('');\n      setOtp('');\n      setName('');\n    };\n\n    // ==================== AUTH FUNCTIONS ====================\n    const handleSendOTP = async () => {\n      if (!phone || phone.length < 10) {\n        Alert.default.alert('Hata', 'Ge√ßerli bir telefon numarasƒ± girin');\n        return;\n      }\n      try {\n        // Cihaz ID'yi al\n        const currentDeviceId = deviceId || (await getOrCreateDeviceId());\n\n        // Kullanƒ±cƒ± kontrol√º yap (cihaz ID ile)\n        const checkResponse = await fetch(`${API_URL}/auth/check-user`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            phone,\n            device_id: currentDeviceId\n          })\n        });\n        const checkData = await checkResponse.json();\n        console.log('üîç Check user response:', checkData);\n        if (checkData.success && checkData.user_exists) {\n          // Kullanƒ±cƒ± kayƒ±tlƒ±\n          setHasPin(checkData.has_pin || false);\n          setUserExists(true);\n          setIsDeviceVerified(checkData.device_verified || false);\n          if (checkData.has_pin && checkData.device_verified) {\n            // PIN var VE cihaz doƒürulanmƒ±≈ü - direkt PIN ekranƒ±na git\n            Alert.default.alert('Ho≈ü Geldiniz! üëã', `${checkData.user_name || 'Kullanƒ±cƒ±'}, 6 haneli ≈üifrenizi girin`);\n            setScreen('enter-pin');\n          } else if (checkData.has_pin && !checkData.device_verified) {\n            // PIN var AMA yeni cihaz - OTP doƒürula sonra PIN iste\n            const response = await fetch(`${API_URL}/auth/send-otp`, {\n              method: 'POST',\n              headers: {\n                'Content-Type': 'application/json'\n              },\n              body: JSON.stringify({\n                phone\n              })\n            });\n            const data = await response.json();\n            if (data.success) {\n              Alert.default.alert('Yeni Cihaz Doƒürulamasƒ± üì±', 'Bu cihazdan ilk giri≈ü. SMS ile g√∂nderilen kodu girin.\\n\\nTest: 123456');\n              setScreen('otp');\n            }\n          } else {\n            // PIN yok - OTP doƒürula ve PIN olu≈ütur\n            const response = await fetch(`${API_URL}/auth/send-otp`, {\n              method: 'POST',\n              headers: {\n                'Content-Type': 'application/json'\n              },\n              body: JSON.stringify({\n                phone\n              })\n            });\n            const data = await response.json();\n            if (data.success) {\n              Alert.default.alert('≈ûifre Olu≈üturma üîê', 'Hesabƒ±nƒ±z i√ßin ≈üifre belirlemeniz gerekiyor. SMS kodunu girin.\\n\\nTest: 123456');\n              setScreen('otp');\n            }\n          }\n        } else {\n          // Yeni kullanƒ±cƒ± - OTP g√∂nder\n          setUserExists(false);\n          const response = await fetch(`${API_URL}/auth/send-otp`, {\n            method: 'POST',\n            headers: {\n              'Content-Type': 'application/json'\n            },\n            body: JSON.stringify({\n              phone\n            })\n          });\n          const data = await response.json();\n          if (data.success) {\n            Alert.default.alert('Kayƒ±t üìù', 'Telefon doƒürulamasƒ± i√ßin SMS kodu g√∂nderildi.\\n\\nTest: 123456');\n            setScreen('otp');\n          }\n        }\n      } catch (error) {\n        console.error('handleSendOTP error:', error);\n        Alert.default.alert('Hata', 'Baƒülantƒ± hatasƒ±');\n      }\n    };\n    const handleVerifyOTP = async () => {\n      if (!otp) {\n        Alert.default.alert('Hata', 'OTP kodunu girin');\n        return;\n      }\n      try {\n        const currentDeviceId = deviceId || (await getOrCreateDeviceId());\n        const response = await fetch(`${API_URL}/auth/verify-otp`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            phone,\n            otp,\n            device_id: currentDeviceId\n          })\n        });\n        const data = await response.json();\n        console.log('üîê Verify OTP response:', data);\n        if (data.success) {\n          if (data.user_exists && data.user) {\n            // Kayƒ±tlƒ± kullanƒ±cƒ±\n            await saveUser(data.user);\n            if (data.has_pin) {\n              // PIN var - PIN giri≈üi\n              setScreen('enter-pin');\n            } else {\n              // PIN yok - PIN olu≈üturmasƒ± lazƒ±m\n              setScreen('set-pin');\n            }\n          } else {\n            // Yeni kullanƒ±cƒ± - Kayƒ±t ekranƒ±\n            setScreen('register');\n          }\n        } else {\n          Alert.default.alert('Hata', data.detail || 'OTP doƒürulanamadƒ±');\n        }\n      } catch (error) {\n        console.error('handleVerifyOTP error:', error);\n        Alert.default.alert('Hata', 'OTP doƒürulanamadƒ±');\n      }\n    };\n    const loadCities = async () => {\n      try {\n        const response = await fetch(`${API_URL}/auth/cities`);\n        const data = await response.json();\n        if (data.success) {\n          setCities(data.cities);\n        }\n      } catch (error) {\n        console.error('≈ûehirler y√ºklenemedi:', error);\n      }\n    };\n    const requestLocationPermission = async () => {\n      try {\n        const {\n          status\n        } = await Location.requestForegroundPermissionsAsync();\n        if (status === 'granted') {\n          setLocationPermission(true);\n          return true;\n        } else {\n          Alert.default.alert('ƒ∞zin Gerekli', 'Konum izni olmadan uygulama √ßalƒ±≈üamaz');\n          return false;\n        }\n      } catch (error) {\n        console.error('Konum izni hatasƒ±:', error);\n        return false;\n      }\n    };\n    const updateUserLocation = async () => {\n      if (!user) return;\n      try {\n        const location = await Location.getCurrentPositionAsync({\n          accuracy: Location.Accuracy.High\n        });\n        const coords = {\n          latitude: location.coords.latitude,\n          longitude: location.coords.longitude\n        };\n        setUserLocation(coords);\n\n        // Backend'e g√∂nder\n        await fetch(`${API_URL}/user/update-location?user_id=${user.id}&latitude=${coords.latitude}&longitude=${coords.longitude}`, {\n          method: 'POST'\n        });\n      } catch (error) {\n        console.error('Konum alƒ±namadƒ±:', error);\n      }\n    };\n    const handleRegister = async () => {\n      if (!name) {\n        Alert.default.alert('Hata', 'Adƒ±nƒ±zƒ± girin');\n        return;\n      }\n      if (!selectedCity) {\n        Alert.default.alert('Hata', '≈ûehir se√ßimi yapmalƒ±sƒ±nƒ±z');\n        return;\n      }\n      try {\n        const response = await fetch(`${API_URL}/auth/register`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            phone,\n            name,\n            city: selectedCity\n          })\n        });\n        const data = await response.json();\n        if (data.success) {\n          await saveUser(data.user);\n          setScreen('role-select'); // Kayƒ±ttan sonra rol se√ßimi\n        } else {\n          Alert.default.alert('Hata', data.detail || 'Kayƒ±t olu≈üturulamadƒ±');\n        }\n      } catch (error) {\n        Alert.default.alert('Hata', 'Kayƒ±t olu≈üturulamadƒ±');\n      }\n    };\n\n    // ==================== RENDER SCREENS ====================\n    if (loading) {\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n        style: styles.container,\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(ActivityIndicator.default, {\n          size: \"large\",\n          color: \"#3FA9F5\"\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 610,\n          columnNumber: 9\n        }, this)\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 609,\n        columnNumber: 7\n      }, this);\n    }\n    if (screen === 'login') {\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n        style: styles.container,\n        children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(AnimatedClouds, {}, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 619,\n          columnNumber: 9\n        }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(ScrollView.default, {\n          contentContainerStyle: styles.scrollContent,\n          children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n            style: styles.logoContainer,\n            children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Logo.default, {\n              size: \"large\",\n              showSlogan: false\n            }, void 0, false, {\n              fileName: _jsxFileName,\n              lineNumber: 623,\n              columnNumber: 13\n            }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n              style: styles.heroTitle,\n              children: \"Yolculuk E\\u015Fle\\u015Ftir