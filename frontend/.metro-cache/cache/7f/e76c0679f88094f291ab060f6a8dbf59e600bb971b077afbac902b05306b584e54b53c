            }, void 0, false, {\n                  fileName: _jsxFileName,\n                  lineNumber: 2987,\n                  columnNumber: 17\n                }, this)\n              }, void 0, false, {\n                fileName: _jsxFileName,\n                lineNumber: 2975,\n                columnNumber: 15\n              }, this)]\n            }, void 0, true, {\n              fileName: _jsxFileName,\n              lineNumber: 2951,\n              columnNumber: 13\n            }, this)]\n          }, void 0, true, {\n            fileName: _jsxFileName,\n            lineNumber: 2938,\n            columnNumber: 11\n          }, this)\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 2937,\n          columnNumber: 9\n        }, this)\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 2931,\n        columnNumber: 7\n      }, this)]\n    }, void 0, true, {\n      fileName: _jsxFileName,\n      lineNumber: 2468,\n      columnNumber: 5\n    }, this);\n  }\n\n  // ==================== DRIVER DASHBOARD ====================\n\n  function DriverDashboard({\n    user,\n    logout,\n    setScreen\n  }) {\n    const [activeTag, setActiveTag] = (0, _react.useState)(null);\n    const [requests, setRequests] = (0, _react.useState)([]);\n    const [loading, setLoading] = (0, _react.useState)(false);\n    const [calling, setCalling] = (0, _react.useState)(false);\n\n    // GPS & Map states\n    const [userLocation, setUserLocation] = (0, _react.useState)(null);\n    const [passengerLocation, setPassengerLocation] = (0, _react.useState)(null);\n\n    // Mesafe ve s√ºre state'leri\n    const [realDistance, setRealDistance] = (0, _react.useState)(0);\n    const [estimatedTime, setEstimatedTime] = (0, _react.useState)(0);\n\n    // Arama state'leri (sesli ve g√∂r√ºnt√ºl√º)\n    const [showVoiceCall, setShowVoiceCall] = (0, _react.useState)(false);\n    const [isVideoCall, setIsVideoCall] = (0, _react.useState)(false);\n    const [selectedPassengerName, setSelectedPassengerName] = (0, _react.useState)('');\n    const [isCallCaller, setIsCallCaller] = (0, _react.useState)(false); // BEN Mƒ∞ ARIYORUM?\n\n    // Gelen arama state'leri\n    const [showIncomingCall, setShowIncomingCall] = (0, _react.useState)(false);\n    const [incomingCallInfo, setIncomingCallInfo] = (0, _react.useState)(null);\n    const [currentCallChannelName, setCurrentCallChannelName] = (0, _react.useState)(null);\n\n    // Kar≈üƒ±lƒ±klƒ± iptal sistemi state'leri - ≈ûOF√ñR\n    const [showTripEndModal, setShowTripEndModal] = (0, _react.useState)(false);\n    const [tripEndRequesterType, setTripEndRequesterType] = (0, _react.useState)(null);\n\n    // Refs for polling (closure problem fix)\n    const callStateRef = (0, _react.useRef)({\n      showVoiceCall: false,\n      showIncomingCall: false,\n      isCallCaller: false\n    });\n    (0, _react.useEffect)(() => {\n      callStateRef.current = {\n        showVoiceCall,\n        showIncomingCall,\n        isCallCaller\n      };\n    }, [showVoiceCall, showIncomingCall, isCallCaller]);\n\n    // Animation\n    const buttonPulse = (0, _react.useRef)(new Animated.default.Value(1)).current;\n    (0, _react.useEffect)(() => {\n      loadData();\n      const interval = setInterval(loadData, 5000);\n      return () => clearInterval(interval);\n    }, [userLocation]); // userLocation deƒüi≈ütiƒüinde de loadData √ßalƒ±≈üsƒ±n\n\n    // Gelen arama polling - ≈ûof√∂r i√ßin\n    (0, _react.useEffect)(() => {\n      // Ba≈ülangƒ±√ß kontrol√º\n      if (!user?.id || !activeTag) return;\n      if (activeTag.status !== 'matched' && activeTag.status !== 'in_progress') return;\n      let isActive = true;\n      const checkIncomingCall = async () => {\n        // G√ºncel state'leri ref'ten oku (closure fix)\n        const {\n          showVoiceCall: inCall,\n          showIncomingCall: hasIncoming,\n          isCallCaller: isCaller\n        } = callStateRef.current;\n\n        // Aramadaysa veya ben arƒ±yorsam polling yapma\n        if (!isActive || inCall || isCaller) return;\n        try {\n          const response = await fetch(`${API_URL}/voice/check-incoming?user_id=${user.id}`);\n          if (!isActive) return;\n          if (!response.ok) return;\n          const text = await response.text();\n          if (!text || text.trim() === '') return;\n          const data = JSON.parse(text);\n\n          // ARAMA Bƒ∞TTƒ∞/ƒ∞PTAL EDƒ∞LDƒ∞ KONTROL√ú\n          if (data.call_ended) {\n            console.log('üìû ≈ûOF√ñR - Arama bitti/iptal edildi:', data.end_reason);\n            setShowIncomingCall(false);\n            setShowVoiceCall(false);\n            setIncomingCallInfo(null);\n            return;\n          }\n\n          // ARAYAN KAPATTI MI KONTROL√ú - IncomingCall a√ßƒ±kken\n          if (hasIncoming && data.success && data.call_cancelled) {\n            console.log('üìû ≈ûOF√ñR - Arayan aramayƒ± kapattƒ±, modal kapatƒ±lƒ±yor');\n            setShowIncomingCall(false);\n            setIncomingCallInfo(null);\n            return;\n          }\n\n          // Gelen arama yoksa ve modal a√ßƒ±ksa kapat (arayan vazge√ßti)\n          if (hasIncoming && data.success && !data.has_incoming) {\n            console.log('üìû ≈ûOF√ñR - Arama artƒ±k yok, modal kapatƒ±lƒ±yor');\n            setShowIncomingCall(false);\n            setIncomingCallInfo(null);\n            return;\n          }\n\n          // Son kontrol - ref'ten g√ºncel deƒüerleri al\n          const currentState = callStateRef.current;\n          if (!isActive || currentState.showVoiceCall || currentState.showIncomingCall) return;\n          if (data.success && data.has_incoming && data.call) {\n            console.log('üìû ≈ûOF√ñR - GELEN ARAMA!', data.call.caller_name);\n            setIncomingCallInfo({\n              callerName: data.call.caller_name,\n              callType: data.call.call_type || 'audio',\n              channelName: data.call.channel_name\n            });\n            setShowIncomingCall(true);\n          }\n        } catch (error) {\n          // Sessiz kal\n        }\n      };\n\n      // ƒ∞lk kontrol√º hemen yap\n      checkIncomingCall();\n      // Sonra her 1.5 saniyede bir kontrol et (daha hƒ±zlƒ± senkronizasyon)\n      const interval = setInterval(checkIncomingCall, 1500);\n      return () => {\n        isActive = false;\n        clearInterval(interval);\n      };\n    }, [user?.id, activeTag?.id, activeTag?.status]);\n\n    // CANLI YOLCU KONUM G√úNCELLEME - E≈üle≈üince ba≈üla\n    (0, _react.useEffect)(() => {\n      if (activeTag && (activeTag.status === 'matched' || activeTag.status === 'in_progress')) {\n        const interval = setInterval(async () => {\n          try {\n            // Yolcu konumunu backend'den al\n            const response = await fetch(`${API_URL}/driver/passenger-location/${activeTag.passenger_id}`);\n            const data = await response.json();\n            if (data.location) {\n              setPassengerLocation(data.location);\n              // Mesafeyi hesapla\n              if (userLocation) {\n                const distance = calculateDistance(userLocation.latitude, userLocation.longitude, data.location.latitude, data.location.longitude);\n                setRealDistance(distance);\n                // Tahmini s√ºreyi hesapla (ortalama 40 km/h)\n                const time = Math.round(distance / 40 * 60);\n                setEstimatedTime(time);\n              }\n            }\n          } catch (error) {\n            console.log('Yolcu konumu alƒ±namadƒ±:', error);\n          }\n        }, 5000); // 5 saniyede bir g√ºncelle\n\n        return () => clearInterval(interval);\n      }\n    }, [activeTag, userLocation]);\n\n    // Kar≈üƒ±lƒ±klƒ± iptal isteƒüi polling - ≈ûOF√ñR i√ßin\n    (0, _react.useEffect)(() => {\n      if (!user?.id || !activeTag) return;\n      if (activeTag.status !== 'matched' && activeTag.status !== 'in_progress') return;\n      const checkTripEndRequest = async () => {\n        try {\n          const response = await fetch(`${API_URL}/trip/check-end-request?tag_id=${activeTag.id}&user_id=${user.id}`);\n          const data = await response.json();\n          if (data.success && data.has_request && !showTripEndModal) {\n            setTripEndRequesterType(data.request.requester_type);\n            setShowTripEndModal(true);\n          }\n        } catch (error) {\n          // Sessiz kal\n        }\n      };\n      checkTripEndRequest();\n      const interval = setInterval(checkTripEndRequest, 3000);\n      return () => clearInterval(interval);\n    }, [user?.id, activeTag?.id, activeTag?.status, showTripEndModal]);\n\n    // GPS konum g√ºncellemesi\n    (0, _react.useEffect)(() => {\n      const updateLocation = async () => {\n        try {\n          const {\n            status\n          } = await Location.requestForegroundPermissionsAsync();\n          if (status === 'granted') {\n            const location = await Location.getCurrentPositionAsync({\n              accuracy: Location.Accuracy.High\n            });\n            const coords = {\n              latitude: location.coords.latitude,\n              longitude: location.coords.longitude\n            };\n            setUserLocation(coords);\n\n            // Backend'e g√∂nder\n            await fetch(`${API_URL}/user/update-location?user_id=${user.id}&latitude=${coords.latitude}&longitude=${coords.longitude}`, {\n              method: 'POST'\n            });\n          }\n        } catch (error) {\n          console.error('Konum alƒ±namadƒ±:', error);\n        }\n      };\n      updateLocation();\n      const locationInterval = setInterval(updateLocation, 10000); // 10 saniyede bir\n      return () => clearInterval(locationInterval);\n    }, [user.id]);\n    const loadData = async () => {\n      await Promise.all([loadActiveTag(), loadRequests()]);\n    };\n    const loadActiveTag = async () => {\n      try {\n        const response = await fetch(`${API_URL}/driver/active-tag?user_id=${user.id}`);\n        const data = await response.json();\n        if (data.success && data.tag) {\n          setActiveTag(data.tag);\n        } else {\n          setActiveTag(null);\n        }\n      } catch (error) {\n        console.error('TAG y√ºklenemedi:', error);\n      }\n    };\n    const loadRequests = async () => {\n      try {\n        // ≈ûof√∂r konumunu da g√∂nder (mesafe hesaplamasƒ± i√ßin)\n        let url = `${API_URL}/driver/requests?user_id=${user.id}`;\n        if (userLocation) {\n          url += `&latitude=${userLocation.latitude}&longitude=${userLocation.longitude}`;\n        }\n        const response = await fetch(url);\n        const data = await response.json();\n        if (data.success) {\n          setRequests(data.requests);\n        }\n      } catch (error) {\n        console.error('Talepler y√ºklenemedi:', error);\n      }\n    };\n    const [offerModalVisible, setOfferModalVisible] = (0, _react.useState)(false);\n    const [selectedTagForOffer, setSelectedTagForOffer] = (0, _react.useState)(null);\n    const [offerPrice, setOfferPrice] = (0, _react.useState)('');\n    const [offerSent, setOfferSent] = (0, _react.useState)(false); // Teklif g√∂nderildi mi?\n\n    const handleSendOffer = tagId => {\n      setSelectedTagForOffer(tagId);\n      setOfferPrice('');\n      setOfferSent(false); // Reset\n      setOfferModalVisible(true);\n    };\n\n    // ≈ûof√∂r i√ßin talebi 10 dakikalƒ±ƒüƒ±na gizle (√ßarpƒ± butonu)\n    const handleDismissRequest = async tagId => {\n      try {\n        const response = await fetch(`${API_URL}/driver/dismiss-request?user_id=${user.id}&tag_id=${tagId}`, {\n          method: 'POST'\n        });\n        const data = await response.json();\n        if (data.success) {\n          // Talebi listeden kaldƒ±r\n          setRequests(prev => prev.filter(r => r.id !== tagId));\n          // Toast g√∂ster\n          Alert.default.alert('Gizlendi', 'Bu talep 10 dakika boyunca g√