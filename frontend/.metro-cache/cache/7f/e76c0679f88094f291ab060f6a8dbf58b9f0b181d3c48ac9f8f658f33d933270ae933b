] = (0, _react.useState)(false);\n\n    // Yeni Auth states\n    const [firstName, setFirstName] = (0, _react.useState)('');\n    const [lastName, setLastName] = (0, _react.useState)('');\n    const [pin, setPin] = (0, _react.useState)('');\n    const [confirmPin, setConfirmPin] = (0, _react.useState)('');\n    const [showPin, setShowPin] = (0, _react.useState)(false);\n    const [userExists, setUserExists] = (0, _react.useState)(false);\n    const [hasPin, setHasPin] = (0, _react.useState)(false);\n    const [deviceId, setDeviceId] = (0, _react.useState)(''); // Cihaz ID\n    const [isDeviceVerified, setIsDeviceVerified] = (0, _react.useState)(false); // Cihaz doÄŸrulanmÄ±ÅŸ mÄ±?\n\n    // Role Selection (Dinamik - Her giriÅŸte seÃ§ilir)\n    const [selectedRole, setSelectedRole] = (0, _react.useState)(null);\n\n    // Animation for role selection\n    const scaleAnim = (0, _react.useRef)(new Animated.default.Value(1)).current;\n\n    // GPS & Map states\n    const [userLocation, setUserLocation] = (0, _react.useState)(null);\n    const [locationPermission, setLocationPermission] = (0, _react.useState)(false);\n\n    // Destination states\n    const [destination, setDestination] = (0, _react.useState)(null);\n    const [showDestinationPicker, setShowDestinationPicker] = (0, _react.useState)(false);\n\n    // Admin Panel state\n    const [isAdmin, setIsAdmin] = (0, _react.useState)(false);\n    const [showAdminPanel, setShowAdminPanel] = (0, _react.useState)(false);\n\n    // Legal Consent state\n    const [showLegalConsent, setShowLegalConsent] = (0, _react.useState)(false);\n    const [legalAccepted, setLegalAccepted] = (0, _react.useState)(false);\n\n    // Legal Pages (Gizlilik, KullanÄ±m ÅžartlarÄ±, KVKK)\n    const [showPrivacy, setShowPrivacy] = (0, _react.useState)(false);\n    const [showTerms, setShowTerms] = (0, _react.useState)(false);\n    const [showKvkk, setShowKvkk] = (0, _react.useState)(false);\n\n    // Push Notifications Hook - Expo Push ile (Fir