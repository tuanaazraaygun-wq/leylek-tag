\n      if (!destination) {\n        console.log('‚ö†Ô∏è Hedef yok!');\n        Alert.default.alert('‚ö†Ô∏è Hedef Gerekli', 'L√ºtfen √∂nce nereye gitmek istediƒüinizi se√ßin');\n        return;\n      }\n      console.log('‚úÖ Hedef var, loading ba≈ülƒ±yor...');\n      setLoading(true);\n      try {\n        // GPS konumu varsa kullan, yoksa mock konum\n        const pickupLat = userLocation?.latitude || 41.0082;\n        const pickupLng = userLocation?.longitude || 28.9784;\n        const response = await fetch(`${API_URL}/passenger/create-request?user_id=${user.id}`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            pickup_location: 'Mevcut Konumunuz',\n            dropoff_location: destination.address,\n            pickup_lat: pickupLat,\n            pickup_lng: pickupLng,\n            dropoff_lat: destination.latitude,\n            dropoff_lng: destination.longitude,\n            notes: 'Hedef belirlendi'\n          })\n        });\n        const data = await response.json();\n        if (data.success) {\n          setActiveTag(data.tag);\n          // Toast notification g√∂ster - otomatik kaybolur\n          setToastMessage('Teklif isteƒüi g√∂nderildi ‚úì');\n          setShowToast(true);\n          setTimeout(() => setShowToast(false), 2500);\n        } else {\n          Alert.default.alert('Hata', data.detail || 'Teklif isteƒüi g√∂nderilemedi');\n        }\n      } catch (error) {\n        Alert.default.alert('Hata', 'Teklif isteƒüi g√∂nderilemedi');\n      } finally {\n        setLoading(false);\n      }\n    };\n\n    // SESLƒ∞ ARAMA - Mock fonksiyon\n    const handleVoiceCall = () => {\n      setCalling(true);\n      Alert.default.alert('üìû Sesli Arama', 'S√ºr√ºc√ºn√ºzle baƒülantƒ± kuruluyor...\\n\\nüîí U√ßtan uca ≈üifreli arama\\nüì± Ger√ßek numaralar gizli', [{\n        text: 'Aramayƒ± Sonlandƒ±r',\n        onPress: () => {\n          setCalling(false);\n          Alert.default.alert('üìû Arama Sonlandƒ±rƒ±ldƒ±', 'Arama ba≈üarƒ±yla sonlandƒ±rƒ±ldƒ±.');\n        }\n      }]);\n    };\n    const handleAcceptOffer = async offerId => {\n      if (!activeTag) return;\n      const selectedOffer = offers.find(o => o.id === offerId);\n      if (!selectedOffer) return;\n      try {\n        const response = await fetch(`${API_URL}/passenger/accept-offer?user_id=${user.id}`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            tag_id: activeTag.id,\n            offer_id: offerId\n          })\n        });\n        const data = await response.json();\n        if (data.success) {\n          // Sadece s√ºr√ºc√º adƒ±nƒ± kaydet, arama ba≈ülatma\n          setSelectedDriverName(selectedOffer.driver_name);\n          loadActiveTag();\n          Alert.default.alert('Ba≈üarƒ±lƒ±', 'E≈üle≈üme saƒülandƒ±! Haritada konumlarƒ± g√∂rebilirsiniz.');\n        }\n      } catch (error) {\n        Alert.default.alert('Hata', 'Teklif kabul edilemedi');\n      }\n    };\n    const handleCancelTag = async () => {\n      if (!activeTag) return;\n      Alert.default.alert('ƒ∞ptal Et', 'ƒ∞steƒüinizi iptal etmek istediƒüinizden emin misiniz? S√ºr√ºc√ºlere bildirim g√∂nderilecek.', [{\n        text: 'Vazge√ß',\n        style: 'cancel'\n      }, {\n        text: 'ƒ∞ptal Et',\n        style: 'destructive',\n        onPress: async () => {\n          try {\n            const response = await fetch(`${API_URL}/passenger/cancel-tag?user_id=${user.id}`, {\n              method: 'POST',\n              headers: {\n                'Content-Type': 'application/json'\n              },\n              body: JSON.stringify({\n                tag_id: activeTag.id\n              })\n            });\n            const data = await response.json();\n            if (data.success) {\n              Alert.default.alert('‚úÖ ƒ∞ptal Edildi', 'ƒ∞steƒüiniz ba≈ü