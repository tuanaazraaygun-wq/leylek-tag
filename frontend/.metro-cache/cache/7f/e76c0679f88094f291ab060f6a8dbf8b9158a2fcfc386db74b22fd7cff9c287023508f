const loadOffers = async tagId => {\n      try {\n        const response = await fetch(`${API_URL}/passenger/offers/${tagId}?user_id=${user.id}`);\n        const data = await response.json();\n        if (data.success) {\n          setOffers(data.offers);\n        }\n      } catch (error) {\n        console.error('Teklifler yÃ¼klenemedi:', error);\n      }\n    };\n\n    // GEÃ‡MÄ°Åž YOLCULUKLARI YÃœKLE\n    const loadTripHistory = async () => {\n      try {\n        const response = await fetch(`${API_URL}/passenger/history?user_id=${user.id}&limit=20`);\n        const data = await response.json();\n        if (data.success) {\n          setTripHistory(data.trips || []);\n        }\n      } catch (error) {\n        console.error('GeÃ§miÅŸ yÃ¼klenemedi:', error);\n      }\n    };\n\n    // PROFÄ°L GÃœNCELLE\n    const updateProfile = async () => {\n      try {\n        const response = await fetch(`${API_URL}/user/update-profile`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            user_id: user.id,\n            name: editName,\n            city: editCity\n          })\n        });\n        const data = await response.json();\n        if (data.success) {\n          Alert.default.alert('BaÅŸarÄ±lÄ±', 'Profil gÃ¼ncellendi');\n          setShowProfileModal(false);\n        } else {\n          Alert.default.alert('Hata', data.detail || 'Profil gÃ¼ncellenemedi');\n        }\n      } catch (error) {\n        Alert.default.alert('Hata', 'Profil gÃ¼ncellenemedi');\n      }\n    };\n\n    // Ã‡AÄžRI BUTONU - Hedef kontrolÃ¼ + koordinat gÃ¶nderimi\n    const handleCallButton = async () => {\n      console.log('ðŸ”µ Ã‡AÄžRI BUTONU TIKLANDI!');\n      console.log('Destination:', destination);\n      console.log('User:', user);\n\n      // Hedef kontrolÃ¼\n      if (!destination) {\n        console.log('âš ï¸ Hedef yok!');\n        Alert.default.alert('âš ï¸ Hedef Gerekli', 'LÃ¼tfen Ã¶nce nereye gitmek istediÄŸinizi seÃ§in');\n        return;\n      }\n      console.log('âœ… Hedef var, loading baÅŸlÄ±yor...');\n      setLoading(true);\n      try {\n        // GPS konumu varsa kullan, yoksa mock konum\n        const pickupLat = userLocation?.latitude || 41.0082;\n        const pickupLng = userLocation?.longitude || 28.9784;\n        const response = await fetch(`${API_URL}/passenger/create-request?user_id=${user.id}`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            pickup_location: 'Mevcut Konumunuz',\n            dropoff_location: destination.address,\n            pickup_lat: pickupLat,\n            pickup_lng: pickupLng,\n            dropoff_lat: destination.latitude,\n            dropoff_lng: destination.longitude,\n            notes: 'Hedef belirlendi'\n          })\n        });\n        const data = await response.json();\n        if (data.success) {\n          setActiveTag(data.tag);\n          // Toast notification gÃ¶ster - otomatik kaybolur\n          setToastMessage('Teklif isteÄŸi gÃ¶nderildi âœ“');\n          setShowToast(true);\n          setTimeout(() => setShowToast(false), 2500);\n        } else {\n          Alert.default.alert('Hata', data.detail || 'Teklif isteÄŸi gÃ¶nderilemedi');\n        }\n      } catch (error) {\n        Alert.default.alert('Hata', 'Teklif isteÄŸi gÃ¶nderilemedi');\n      } finally {\n        setLoading(false);\n      }\n    };\n\n    // SESLÄ° ARAMA - Mock fonksiyon\n    const handleVoiceCall = () => {\n      setCalling(true);\n      Alert.default.alert('ðŸ“ž Sesli Arama', 'SÃ¼rÃ¼cÃ¼nÃ¼zle baÄŸlantÄ± kuruluyor...\\n\\nðŸ”’ UÃ§tan uca ÅŸifreli arama\\nðŸ“± GerÃ§ek numaralar gizli', [{\n        text: 'AramayÄ± SonlandÄ±r',\n        onPress: () => {\n          setCalling(false);\n        }\n      }]);\n    };\n\n    // Teklifi 10 dakikalÄ±ÄŸÄ±na gizle (Ã§arpÄ± butonu)\n    const handleDi