: refetchOffers\n    } = (0, _hooksUseOffers.useOffers)({\n      userId: user?.id || '',\n      tagId: activeTag?.id,\n      isDriver: false,\n      enabled: !!(user?.id && activeTag?.id && (activeTag?.status === 'pending' || activeTag?.status === 'offers_received')),\n      onNewOffer: offer => {\n        console.log('ðŸ”” YENÄ° TEKLÄ°F GELDÄ° (Realtime):', offer.price, 'TL');\n        // Bildirim gÃ¶ster\n        setToastMessage(`Yeni teklif: ${offer.price} TL`);\n        setShowToast(true);\n        setTimeout(() => setShowToast(false), 3000);\n      }\n    });\n\n    // Teklifleri fiyata gÃ¶re sÄ±rala (ucuzdan pahalÄ±ya)\n    const offers = [...realtimeOffers].sort((a, b) => (a.price || 0) - (b.price || 0));\n\n    // Mesafe ve sÃ¼re state'leri\n    const [realDistance, setRealDistance] = (0, _react.useState)(0);\n    const [estimatedTime, setEstimatedTime] = (0, _react.useState)(0);\n\n    // Arama state'leri (sesli ve gÃ¶rÃ¼ntÃ¼lÃ¼)\n    const [showVoiceCall, setShowVoiceCall] = (0, _react.useState)(false);\n    const [isVideoCall, setIsVideoCall] = (0, _react.useState)(false);\n    const [selectedDriverName, setSelectedDriverName] = (0, _react.useState)('');\n    const [isCallCaller, setIsCallCaller] = (0, _react.useState)(false); // BEN MÄ° ARIYORUM?\n    const [activeChannelName, setActiveChannelName] = (0, _react.useState)(''); // ARAMA KANAL ADI\n    const [activeCallId, setActiveCallId] = (0, _react.useState)(''); // ARAMA ID\n\n    // Gelen arama state'leri\n    const [showIncomingCall, setShowIncomingCall] = (0, _react.useState)(false);\n    const [incomingCallInfo, setIncomingCallInfo] = (0, _react.useState)(null);\n\n    // KarÅŸÄ±lÄ±klÄ± iptal sistemi state'leri\n    const [showTripEndModal, setShowTripEndModal] = (0, _react.useState)(false);\n    const [tripEndRequesterType, setTripEndRequesterType] = (0, _react.useState)(null);\n\n    // Refs for polling (closure problem fix) - YOLCU\n    const passengerCallStateRef = (0, _react.useRef)({\n      s