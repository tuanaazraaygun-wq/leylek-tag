        }, this)]\n          }, void 0, true, {\n            fileName: _jsxFileName,\n            lineNumber: 2914,\n            columnNumber: 11\n          }, this)\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 2913,\n          columnNumber: 9\n        }, this)\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 2907,\n        columnNumber: 7\n      }, this)]\n    }, void 0, true, {\n      fileName: _jsxFileName,\n      lineNumber: 2467,\n      columnNumber: 5\n    }, this);\n  }\n\n  // ==================== DRIVER DASHBOARD ====================\n\n  function DriverDashboard({\n    user,\n    logout,\n    setScreen\n  }) {\n    const [activeTag, setActiveTag] = (0, _react.useState)(null);\n    const [requests, setRequests] = (0, _react.useState)([]);\n    const [loading, setLoading] = (0, _react.useState)(false);\n    const [calling, setCalling] = (0, _react.useState)(false);\n\n    // GPS & Map states\n    const [userLocation, setUserLocation] = (0, _react.useState)(null);\n    const [passengerLocation, setPassengerLocation] = (0, _react.useState)(null);\n\n    // Mesafe ve s√ºre state'leri\n    const [realDistance, setRealDistance] = (0, _react.useState)(0);\n    const [estimatedTime, setEstimatedTime] = (0, _react.useState)(0);\n\n    // Arama state'leri (sesli ve g√∂r√ºnt√ºl√º)\n    const [showVoiceCall, setShowVoiceCall] = (0, _react.useState)(false);\n    const [isVideoCall, setIsVideoCall] = (0, _react.useState)(false);\n    const [selectedPassengerName, setSelectedPassengerName] = (0, _react.useState)('');\n    const [isCallCaller, setIsCallCaller] = (0, _react.useState)(false); // BEN Mƒ∞ ARIYORUM?\n\n    // Gelen arama state'leri\n    const [showIncomingCall, setShowIncomingCall] = (0, _react.useState)(false);\n    const [incomingCallInfo, setIncomingCallInfo] = (0, _react.useState)(null);\n\n    // Kar≈üƒ±lƒ±klƒ± iptal sistemi state'leri - ≈ûOF√ñR\n    const [showTripEndModal, setShowT