\n  }) {\n    const [activeTag, setActiveTag] = (0, _react.useState)(null);\n    const [loading, setLoading] = (0, _react.useState)(false);\n    const [calling, setCalling] = (0, _react.useState)(false);\n    const [currentOfferIndex, setCurrentOfferIndex] = (0, _react.useState)(0);\n    const [showArrowHint, setShowArrowHint] = (0, _react.useState)(false);\n    const [driverLocation, setDriverLocation] = (0, _react.useState)(null);\n\n    // Toast notification state\n    const [showToast, setShowToast] = (0, _react.useState)(false);\n    const [toastMessage, setToastMessage] = (0, _react.useState)('');\n\n    // Ses efekti iÃ§in\n    const soundRef = (0, _react.useRef)(null);\n\n    // Teklif geldiÄŸinde ses Ã§al\n    const playOfferSound = async () => {\n      try {\n        // Ã–nceki sesi durdur\n        if (soundRef.current) {\n          await soundRef.current.unloadAsync();\n        }\n\n        // Yeni ses yÃ¼kle ve Ã§al - casino/slot machine tarzÄ± ses\n        const {\n          sound\n        } = await _expoAv.Audio.Sound.createAsync({\n          uri: 'https://assets.mixkit.co/active_storage/sfx/2019/2019-preview.mp3'\n        }, {\n          shouldPlay: true,\n          volume: 1.0\n        });\n        soundRef.current = sound;\n\n        // Ses bitince temizle\n        sound.setOnPlaybackStatusUpdate(status => {\n          if (status.isLoaded && status.didJustFinish) {\n            sound.unloadAsync();\n          }\n        });\n      } catch (error) {\n        console.log('Ses Ã§alma hatasÄ±:', error);\n      }\n    };\n\n    // ========== SUPABASE REALTIME - TEKLÄ°F YÃ–NETÄ°MÄ° ==========\n    // useOffers hook'u ile anlÄ±k teklif gÃ¼ncellemeleri (polling yerine)\n    const {\n      offers: realtimeOffers,\n      isLoading: offersLoading,\n      acceptOffer: acceptOfferRealtime,\n      rejectOffer: rejectOfferRealtime,\n      refetch: refetchOffers\n    } = (0, _hooksUseOffers.useOffers)({\n      userId: user?.id || '',\n      tagId: activeTag?.id,\n 