} catch (error) {\n        console.error('Talepler y√ºklenemedi:', error);\n      }\n    };\n    const [offerModalVisible, setOfferModalVisible] = (0, _react.useState)(false);\n    const [selectedTagForOffer, setSelectedTagForOffer] = (0, _react.useState)(null);\n    const [offerPrice, setOfferPrice] = (0, _react.useState)('');\n    const [offerSent, setOfferSent] = (0, _react.useState)(false); // Teklif g√∂nderildi mi?\n\n    const handleSendOffer = tagId => {\n      setSelectedTagForOffer(tagId);\n      setOfferPrice('');\n      setOfferSent(false); // Reset\n      setOfferModalVisible(true);\n    };\n\n    // ≈ûof√∂r i√ßin talebi 10 dakikalƒ±ƒüƒ±na gizle (√ßarpƒ± butonu)\n    const handleDismissRequest = async tagId => {\n      try {\n        const response = await fetch(`${API_URL}/driver/dismiss-request?user_id=${user.id}&tag_id=${tagId}`, {\n          method: 'POST'\n        });\n        const data = await response.json();\n        if (data.success) {\n          // Talebi listeden kaldƒ±r\n          setRequests(prev => prev.filter(r => r.id !== tagId));\n          // Toast g√∂ster\n          Alert.default.alert('Gizlendi', 'Bu talep 10 dakika boyunca g√∂r√ºnmeyecek');\n        }\n      } catch (error) {\n        console.log('Dismiss error:', error);\n      }\n    };\n    const submitOffer = async () => {\n      if (!offerPrice || isNaN(Number(offerPrice))) {\n        Alert.default.alert('Hata', 'Ge√ßerli bir fiyat girin');\n        return;\n      }\n      try {\n        const response = await fetch(`${API_URL}/driver/send-offer?user_id=${user.id}`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            tag_id: selectedTagForOffer,\n            price: Number(offerPrice),\n            estimated_time: 15,\n            notes: 'Hemen geliyorum!'\n          })\n        });\n        const data = await response.json();\n        if (data.success) {\n          // But