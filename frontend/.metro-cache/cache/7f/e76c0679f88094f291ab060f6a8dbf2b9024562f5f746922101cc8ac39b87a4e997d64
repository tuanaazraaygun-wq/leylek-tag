\n      }\n    }, [notification]);\n\n    // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n    // PERMISSION GATE - Request ALL permissions at app start\n    // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n    const requestAllPermissions = async () => {\n      if (Platform.default.OS !== 'android') {\n        console.log('ðŸ” iOS - izinler otomatik isteniyor');\n        setMicrophonePermission(true);\n        setCameraPermission(true);\n        setPermissionsGranted(true);\n        setPermissionChecking(false);\n        return true;\n      }\n      console.log('ðŸ” Android - TÃ¼m izinler isteniyor...');\n      try {\n        // Core permissions\n        const permissions = [_reactNativeWebDistIndex.PermissionsAndroid.PERMISSIONS.RECORD_AUDIO, _reactNativeWebDistIndex.PermissionsAndroid.PERMISSIONS.CAMERA];\n        console.log('ðŸ” Ä°stenen izinler:', permissions);\n        const results = await _reactNativeWebDistIndex.PermissionsAndroid.requestMultiple(permissions);\n        console.log('ðŸ” Ä°zin sonuÃ§larÄ±:', JSON.stringify(results, null, 2));\n        const audioGranted = results[_reactNativeWebDistIndex.PermissionsAndroid.PERMISSIONS.RECORD_AUDIO] === 'granted';\n        const cameraGranted = results[_reactNativeWebDistIndex.PermissionsAndroid.PERMISSIONS.CAMERA] === 'granted';\n        setMicrophonePermission(audioGranted);\n        setCameraPermission(cameraGranted);\n\n        // En Ã¶nemli izin: RECORD_AUDIO - bu olmadan arama yapÄ±lamaz\n        if (!audioGranted) {\n          console.log('âŒ RECORD_AUDIO izni REDDEDÄ°LDÄ° - Arama yapÄ±lamaz!');\n          Alert.default.alert('Mikrofon Ä°zni Gerekli', 'Sesli ve gÃ¶rÃ¼ntÃ¼lÃ¼ arama yapabilmek iÃ§in mikrofon izni vermeniz gerekiyor. LÃ¼tfen ayarlardan izin verin.', [{\n            text: 'Tamam',\n            onPress: () => Linking.default.openSettings()\n          }]);\n          setPermissionsGranted(false);\n          setPermissionChecking(false);\n          return false;\n        }\n        console.log('âœ… RECORD_AUDIO izni verildi');\n        console.log(cameraGranted ? 'âœ… CAMERA izni verildi' : 'âš ï¸ CAMERA izni reddedildi (opsiyonel)');\n        setPermissionsGranted(true);\n        setPermissionChecking(false);\n        return true;\n      } catch (error) {\n        console.error('ðŸ” Ä°zin hatasÄ±:', error);\n        setPermissionChecking(false);\n        return false;\n      }\n    };\n\n    // Uygulama baÅŸlangÄ±cÄ±nda izinleri iste - HEMEN\n    (0, _react.useEffect)(() => {\n      const checkPermissions = async () => {\n        console.log('ðŸ” Uygulama baÅŸladÄ± - Ä°zin kontrolÃ¼ baÅŸlÄ±yor...');\n        await requestAllPermissions();\n      };\n\n      // Splash screen beklemeden hemen izin iste\n      checkPermissions();\n    }, []);\n\n    // Device ID oluÅŸtur veya al\n    const getOrCreateDeviceId = async () => {\n      try {\n        let storedDeviceId = await AsyncStorage.default.getItem('device_id');\n        if (!storedDeviceId) {\n          // Yeni cihaz ID oluÅŸtur\n          storedDeviceId = 'device_' + Date.now() + '_' + Math.random().toString(36).substring(2, 15);\n          await AsyncStorage.default.setItem('device_id', storedDeviceId);\n          console.log('ðŸ†” Yeni cihaz ID oluÅŸturuldu:', storedDeviceId);\n        } else {\n          console.log('ðŸ†” Mevcut cihaz ID:', storedDeviceId);\n        }\n        return storedDeviceId;\n      } catch (error) {\n        console.error('Device ID hatasÄ±:', error);\n        return 'device_' + Date.now();\n      }\n    };\n    (0, _react.useEffect)(() => {\n      initializeApp();\n    }, []);\n    const initializeApp = async () => {\n      // Ã–nce cihaz ID'yi al\n      const dId = await getOrCreateDeviceId();\n      setDeviceId(dId);\n\n      // KVKK onayÄ± kontrolÃ¼ (telefon num