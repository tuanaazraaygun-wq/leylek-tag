results = await _reactNativeWebDistIndex.PermissionsAndroid.requestMultiple(permissions);\n        console.log('ðŸ” Ä°zin sonuÃ§larÄ±:', JSON.stringify(results, null, 2));\n        const audioGranted = results[_reactNativeWebDistIndex.PermissionsAndroid.PERMISSIONS.RECORD_AUDIO] === 'granted';\n        const cameraGranted = results[_reactNativeWebDistIndex.PermissionsAndroid.PERMISSIONS.CAMERA] === 'granted';\n        setMicrophonePermission(audioGranted);\n        setCameraPermission(cameraGranted);\n\n        // En Ã¶nemli izin: RECORD_AUDIO - bu olmadan arama yapÄ±lamaz\n        if (!audioGranted) {\n          console.log('âŒ RECORD_AUDIO izni REDDEDÄ°LDÄ° - Arama yapÄ±lamaz!');\n          Alert.default.alert('Mikrofon Ä°zni Gerekli', 'Sesli ve gÃ¶rÃ¼ntÃ¼lÃ¼ arama yapabilmek iÃ§in mikrofon izni vermeniz gerekiyor. LÃ¼tfen ayarlardan izin verin.', [{\n            text: 'Tamam',\n            onPress: () => Linking.default.openSettings()\n          }]);\n          setPermissionsGranted(false);\n          setPermissionChecking(false);\n          return false;\n        }\n        console.log('âœ… RECORD_AUDIO izni verildi');\n        console.log(cameraGranted ? 'âœ… CAMERA izni verildi' : 'âš ï¸ CAMERA izni reddedildi (opsiyonel)');\n        setPermissionsGranted(true);\n        setPermissionChecking(false);\n        return true;\n      } catch (error) {\n        console.error('ðŸ” Ä°zin hatasÄ±:', error);\n        setPermissionChecking(false);\n        return false;\n      }\n    };\n\n    // Uygulama baÅŸlangÄ±cÄ±nda izinleri iste - HEMEN\n    (0, _react.useEffect)(() => {\n      const checkPermissions = async () => {\n        console.log('ðŸ” Uygulama baÅŸladÄ± - Ä°zin kontrolÃ¼ baÅŸlÄ±yor...');\n        await requestAllPermissions();\n      };\n\n      // Splash screen beklemeden hemen izin iste\n      checkPermissions();\n    }, []);\n\n    // Device ID oluÅŸtur veya al\n    const getOrCreateDeviceId = async () => {\n      try {\n        let storedDeviceId = await AsyncStorage.default.getItem('device_id');\n        if (!storedDeviceId) {\n          // Yeni cihaz ID oluÅŸtur\n          storedDeviceId = 'device_' + Date.now() + '_' + Math.random().toString(36).substring(2, 15);\n          await AsyncStorage.default.setItem('device_id', storedDeviceId);\n          console.log('ðŸ†” Yeni cihaz ID oluÅŸturuldu:', storedDeviceId);\n        } else {\n          console.log('ðŸ†” Mevcut cihaz ID:', storedDeviceId);\n        }\n        return storedDeviceId;\n      } catch (error) {\n        console.error('Device ID hatasÄ±:', error);\n        return 'device_' + Date.now();\n      }\n    };\n    (0, _react.useEffect)(() => {\n      initializeApp();\n    }, []);\n    const initializeApp = async () => {\n      // Ã–nce cihaz ID'yi al\n      const dId = await getOrCreateDeviceId();\n      setDeviceId(dId);\n\n      // KVKK onayÄ± kontrolÃ¼ (telefon numarasÄ±na gÃ¶re)\n      const storedKVKKPhone = await AsyncStorage.default.getItem('kvkk_accepted_phone');\n      // Onay varsa set et\n      if (storedKVKKPhone) {\n        setKvkkAccepted(true);\n      }\n\n      // Sonra kullanÄ±cÄ±yÄ± yÃ¼kle\n      await loadUser();\n    };\n\n    // KVKK onayÄ±nÄ± kaydet (telefon numarasÄ±na gÃ¶re)\n    const saveKVKKConsent = async phoneNumber => {\n      try {\n        await AsyncStorage.default.setItem('kvkk_accepted_phone', phoneNumber);\n        setKvkkAccepted(true);\n        console.log('âœ… KVKK onayÄ± kaydedildi:', phoneNumber);\n      } catch (error) {\n        console.error('KVKK kayÄ±t hatasÄ±:', error);\n      }\n    };\n\n    // KVKK onayÄ± kontrol (telefon deÄŸiÅŸtiÄŸinde yeniden iste)\n    const checkKVKKConsent = async phoneNumber => {\n      try {\n        const storedPhone = await AsyncStorage.default.getItem('kvkk_accepted_phone');\n        return storedPhone === phoneNumber;\n      } catch (error) {\n        return false;\n      }\n    };\n    (0, _react.useEffect)(() => {\n      if (user && screen === 'dashboard') {\n        requestLocationPermission().then(granted => {\n          if (granted) {\n            updateUserLocation();\n            // Her 0.5 saniyede bir konum gÃ¼ncelle - CANLI TAKÄ°P\n            const interval = setInterval(updateUserLocation, 500);\n            return () => clearInterval(interval);\n          }\n        });\n      }\n    }, [user, screen]);\n    const loadUser = async () => {\n      try {\n        const userData = await AsyncStorage.default.getItem('user');\n        const legalAcceptedStorage = await AsyncStorage.default.getItem('legal_accepted');\n        if (userData) {\n          const parsedUser = JSON.parse(userData);\n          setUser(parsedUser);\n\n          // Admin kontrolÃ¼ - 5326497412 ana admin\n          const cleanPhone = parsedUser.phone?.replace(/\\D/g, '');\n          const isMainAdmin = cleanPhone === '5326497412' || cleanPhone === '05326497412';\n          if (isMainAdmin) {\n            setIsAdmin(true);\n            // Admin direkt admin paneline gitsin\n            setShowAdminPanel(true);\n            setScreen('role-select');\n          } else {\n            // API'den admin kontrolÃ¼\n            try {\n              const res = await fetch(`${API_URL}/admin/check?phone=${cleanPhone}`);\n              const data = await res.json();\n              if (data.success && data.is_admin) {\n                setIsAdmin(true);\n                setShowAdminPanel(true); // DiÄŸer adminler de direkt panele\n              }\n            } catch (e) {}\n            setScreen('role-select');\n          }\n\n          // Legal consent kontrolÃ¼\n          if (legalAcceptedStorage !== 'true') {\n            setShowLegalConsent(true);\n          } else {\n            setLegalAccepted(true);\n          }\n        }\n      } catch (error) {\n        console.error('KullanÄ±cÄ± yÃ¼klenemedi:', error);\n      } finally {\n        setLoading(false);\n      }\n    };\n\n    // Legal consent kabul\n    const handleLegalAccept = async () => {\n      await AsyncStorage.default.setItem('legal_accepted', 'true');\n      setLegalAccepted(true);\n      setShowLegalConsent(false);\n    };\n\n    // Legal consent red\n    const handleLegalDecline = async () => {\n      Alert.default.alert('UyarÄ±', 'KullanÄ±m ÅŸartlarÄ±nÄ± kabul etmeden devam edemezsiniz.', [{\n        text: 'Tamam'\n      }]);\n    };\n    const saveUser = async userData => {\n      await AsyncStorage.default.setItem('user', JSON.stringify(userData));\n      setUser(userData);\n    };\n    const logout = async () => {\n      // Logout sÄ±rasÄ±nda push token'Ä± sil\n      if (user?.id) {\n        await removePushToken(user.id);\n      }\n      await AsyncStorage.default.removeItem('user');\n      setUser(null);\n      setScreen('login');\n      setPhone('');\n      setOtp('');\n      setName('');\n    };\n\n    // ==================== AUTH FUNCTIONS ====================\n    const handleSendOTP = async () => {\n      if (!phone || phone.length < 10) {\n        Alert.default.alert('Hata', 'GeÃ§erli bir telefon numarasÄ± girin');\n        return;\n      }\n      try {\n        // Cihaz ID'yi al\n        const currentDeviceId = deviceId || (await getOrCreateDeviceId());\n\n        // KullanÄ±cÄ± kontrolÃ¼ yap\n        const checkResponse = await fetch(`${API_URL}/auth/check-user`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            phone,\n            device_id: currentDeviceId\n          })\n        });\n        const checkData = await checkResponse.json();\n        console.log('ðŸ” Check user response:', checkData);\n        if (checkData.success && checkData.user_exists && checkData.has_pin) {\n          // âœ… KAYITLI KULLANICI VE PIN'Ä° VAR - DÄ°REKT PIN EKRANINA GÄ°T (OTP YOK!)\n          setHasPin(true);\n          setUserExists(true);\n          setIsDeviceVerified(true);\n          Alert.default.alert('HoÅŸ Geldiniz! ðŸ‘‹', `${checkData.user_name || 'KullanÄ±cÄ±'}, 6 haneli ÅŸifrenizi girin`);\n          setScreen('enter-pin');\n        } else if (checkData.success && checkData.user_exists && !checkData.has_pin) {\n          // KayÄ±tlÄ± ama PIN yok - OTP gÃ¶nder ve PIN oluÅŸtur\n          setUserExists(true);\n          setHasPin(false);\n          const response = await fetch(`${API_URL}/auth/send-otp`, {\n            method: 'POST',\n            headers: {\n              'Content-Type': 'application/json'\n            },\n            body: JSON.stringify({\n              phone\n            })\n          });\n          const data = await response.json();\n          if (data.success) {\n            Alert.default.alert('Åžifre OluÅŸturma ðŸ”', 'HesabÄ±nÄ±z iÃ§in 6 haneli ÅŸifre belirlemeniz gerekiyor. SMS kodunu girin.\\n\\nTest: 123456');\n            setScreen('otp');\n          }\n        } else {\n          // ðŸ†• YENÄ° KULLANICI - OTP ile kayÄ±t\n          setUserExists(false);\n          setHasPin(false);\n          const response = await fetch(`${API_URL}/auth/send-otp`, {\n            method: 'POST',\n            headers: {\n              'Content-Type': 'application/json'\n            },\n            body: JSON.stringify({\n              phone\n            })\n          });\n          const data = await response.json();\n          if (data.success) {\n            Alert.default.alert('KayÄ±t ðŸ“', 'Telefon doÄŸrulamasÄ± iÃ§in SMS kodu gÃ¶nderildi.\\n\\nTest: 123456');\n            setScreen('otp');\n          }\n        }\n      } catch (error) {\n        console.error('handleSendOTP error:', error);\n        Alert.default.alert('Hata', 'BaÄŸlantÄ± hatasÄ±');\n      }\n    };\n    const handleVerifyOTP = async () => {\n      if (!otp) {\n        Alert.default.alert('Hata', 'OTP kodunu girin');\n        return;\n      }\n      try {\n        const currentDeviceId = deviceId || (await getOrCreateDeviceId());\n        const response = await fetch(`${API_URL}/auth/verify-otp`, {\n          method: 'POST',\