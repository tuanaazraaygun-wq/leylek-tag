(newDestination);\n      setShowDestinationPicker(false);\n\n      // EÄŸer aktif TAG varsa, hedefi gÃ¼ncelle\n      if (activeTag) {\n        try {\n          const response = await fetch(`${API_URL}/passenger/update-destination?user_id=${user.id}`, {\n            method: 'POST',\n            headers: {\n              'Content-Type': 'application/json'\n            },\n            body: JSON.stringify({\n              tag_id: activeTag.id,\n              dropoff_location: address,\n              dropoff_lat: lat,\n              dropoff_lng: lng\n            })\n          });\n          const data = await response.json();\n          if (data.success) {\n            loadActiveTag(); // TAG'i yeniden yÃ¼kle\n          } else {\n            Alert.default.alert('Hata', data.detail || 'Hedef gÃ¼ncellenemedi');\n          }\n        } catch (error) {\n          Alert.default.alert('Hata', 'Hedef gÃ¼ncellenemedi');\n        }\n      }\n    };\n\n    // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n    // ðŸ†• SEARCHING PHASE - HARÄ°TA + TEKLÄ°F LÄ°STESÄ° (YENÄ° UI)\n    // Ãœstte harita (tÃ¼m sÃ¼rÃ¼cÃ¼ler) + Altta scrollable teklif listesi\n    // â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\n    if (activeTag && activeTag.status !== 'matched' && activeTag.status !== 'in_progress') {\n      const isSearching = activeTag.status === 'pending' || activeTag.status === 'offers_received';\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n        style: searchingStyles.container,\n        children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n          style: searchingStyles.topBar,\n          children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(TouchableOpacity.default, {\n            onPress: () 