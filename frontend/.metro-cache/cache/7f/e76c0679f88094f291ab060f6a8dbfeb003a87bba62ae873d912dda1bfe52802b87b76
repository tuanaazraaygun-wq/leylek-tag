});\n\n    // KarÅŸÄ±lÄ±klÄ± iptal sistemi state'leri - ÅžOFÃ–R\n    const [showTripEndModal, setShowTripEndModal] = (0, _react.useState)(false);\n    const [tripEndRequesterType, setTripEndRequesterType] = (0, _react.useState)(null);\n\n    // Animation\n    const buttonPulse = (0, _react.useRef)(new Animated.default.Value(1)).current;\n    (0, _react.useEffect)(() => {\n      console.log('ðŸ”„ SÃ¼rÃ¼cÃ¼ polling baÅŸlatÄ±ldÄ±');\n      loadData();\n      const interval = setInterval(() => {\n        loadData();\n      }, 3000);\n      return () => {\n        console.log('ðŸ”„ SÃ¼rÃ¼cÃ¼ polling durduruldu');\n        clearInterval(interval);\n      };\n    }, [user?.id]);\n\n    // CANLI YOLCU KONUM GÃœNCELLEME - EÅŸleÅŸince baÅŸla\n    (0, _react.useEffect)(() => {\n      if (activeTag && (activeTag.status === 'matched' || activeTag.status === 'in_progress')) {\n        const interval = setInterval(async () => {\n          try {\n            // Yolcu konumunu backend'den al\n            const response = await fetch(`${API_URL}/driver/passenger-location/${activeTag.passenger_id}`);\n            const data = await response.json();\n            if (data.location) {\n              setPassengerLocation(data.location);\n              // Mesafeyi hesapla\n              if (userLocation) {\n                const distance = calculateDistance(userLocation.latitude, userLocation.longitude, data.location.latitude, data.location.longitude);\n                setRealDistance(distance);\n                // Tahmini sÃ¼reyi hesapla (ortalama 40 km/h)\n                const time = Math.round(distance / 40 * 60);\n                setEstimatedTime(time);\n              }\n            }\n          } catch (error) {\n            console.log('Yolcu konumu alÄ±namadÄ±:', error);\n          }\n        }, 5000); // 5 saniyede bir gÃ¼ncelle\n\n        return () => clearInterval(interval);\n      }\n    }, [activeTag, userLocation]);\n\n    // KarÅŸÄ±lÄ±klÄ± iptal isteÄŸi polling - ÅžOFÃ–R iÃ§