\n        const data = await response.json();\n        if (data.success) {\n          await saveUser(data.user);\n          setScreen('role-select'); // KayÄ±ttan sonra rol seÃ§imi\n        } else {\n          Alert.default.alert('Hata', data.detail || 'KayÄ±t oluÅŸturulamadÄ±');\n        }\n      } catch (error) {\n        Alert.default.alert('Hata', 'KayÄ±t oluÅŸturulamadÄ±');\n      }\n    };\n\n    // ==================== RENDER SCREENS ====================\n\n    // SPLASH SCREEN - 3 saniye gÃ¶ster - daha gÃ¼venilir timeout ile\n    (0, _react.useEffect)(() => {\n      if (showSplash) {\n        const splashTimer = setTimeout(() => {\n          console.log('ðŸŽ¬ Splash timeout - login ekranÄ±na geÃ§iliyor');\n          setShowSplash(false);\n          if (!user) {\n            setScreen('login');\n          }\n        }, 3000);\n        return () => clearTimeout(splashTimer);\n      }\n    }, [showSplash, user]);\n    if (showSplash) {\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(SplashScreen.default, {\n        onFinish: () => {\n          console.log('ðŸŽ¬ SplashScreen onFinish Ã§aÄŸrÄ±ldÄ±');\n          setShowSplash(false);\n          if (!user) {\n            setScreen('login');\n          }\n        }\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 658,\n        columnNumber: 7\n      }, this);\n    }\n    if (loading) {\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n        style: styles.container,\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(ActivityIndicator.default, {\n          size: \"large\",\n          color: \"#3FA9F5\"\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 671,\n          columnNumber: 9\n        }, this)\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 670,\n        columnNumber: 7\n      }, this);\n    }\n    if (screen === 'login') {\n      r