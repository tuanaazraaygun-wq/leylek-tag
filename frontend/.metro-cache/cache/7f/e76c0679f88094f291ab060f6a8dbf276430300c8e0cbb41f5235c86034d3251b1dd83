Button,\n                onPress: async () => {\n                  try {\n                    const response = await fetch(`${API_URL}/trip/respond-end-request?tag_id=${activeTag?.id}&user_id=${user.id}&approved=true`, {\n                      method: 'POST'\n                    });\n                    const data = await response.json();\n                    if (data.success && data.approved) {\n                      Alert.default.alert('‚úÖ Yolculuk Tamamlandƒ±', 'Yolculuk kar≈üƒ±lƒ±klƒ± onay ile sonlandƒ±rƒ±ldƒ±.');\n                      setActiveTag(null);\n                      loadActiveTag();\n                    }\n                  } catch (error) {\n                    Alert.default.alert('Hata', 'ƒ∞≈ülem ba≈üarƒ±sƒ±z');\n                  }\n                  setShowTripEndModal(false);\n                },\n                children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n                  style: styles.tripEndApproveButtonText,\n                  children: \"Onayl\\u0131yorum\"\n                }, void 0, false, {\n                  fileName: _jsxFileName,\n                  lineNumber: 2919,\n                  columnNumber: 17\n                }, this)\n              }, void 0, false, {\n                fileName: _jsxFileName,\n                lineNumber: 2899,\n                columnNumber: 15\n              }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(TouchableOpacity.default, {\n                style: styles.tripEndRejectButton,\n                onPress: async () => {\n                  try {\n                    await fetch(`${API_URL}/trip/respond-end-request?tag_id=${activeTag?.id}&user_id=${user.id}&approved=false`, {\n                      method: 'POST'\n                    });\n                  } catch (error) {}\n                  setShowTripEndModal(false);\n                },\n                children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n                  style: styles.tripEndRejec