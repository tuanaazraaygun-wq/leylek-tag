 = (0, _react.useState)('');\n    const [incomingCallTagId, setIncomingCallTagId] = (0, _react.useState)(''); // ðŸ†• Gelen arama tag_id\n\n    // Giden Arama State (Araniyor...) - SOFOR\n    const [outgoingCall, setOutgoingCall] = (0, _react.useState)(false);\n    const [outgoingCallData, setOutgoingCallData] = (0, _react.useState)(null);\n\n    // ðŸ†• Chat State'leri (SÃ¼rÃ¼cÃ¼)\n    const [driverChatVisible, setDriverChatVisible] = (0, _react.useState)(false);\n    const [driverChatMessages, setDriverChatMessages] = (0, _react.useState)([]);\n\n    // Eski Agora state'leri (artik kullanilmiyor ama kaldirilmadi)\n    const [showCallScreen, setShowCallScreen] = (0, _react.useState)(false);\n    const [callScreenData, setCallScreenData] = (0, _react.useState)(null);\n\n    // Socket.IO arama durumlari\n    const [callAccepted, setCallAccepted] = (0, _react.useState)(false);\n    const [callRejected, setCallRejected] = (0, _react.useState)(false);\n    const [callEnded, setCallEnded] = (0, _react.useState)(false);\n    const [receiverOffline, setReceiverOffline] = (0, _react.useState)(false);\n\n    // Arama kilidi\n    const isCallActiveRef = (0, _react.useRef)(false);\n\n    // ==================== SOCKET.IO HOOK - ÅžOFÃ–R ====================\n    const {\n      socket: driverSocket,\n      // ðŸ†• Socket instance\n      isConnected: socketConnected,\n      isRegistered: socketRegistered,\n      startCall: socketStartCall,\n      acceptCall: socketAcceptCall,\n      rejectCall: socketRejectCall,\n      endCall: socketEndCall,\n      // TAG & Teklif iÃ§in yeni fonksiyonlar\n      emitSendOffer: socketSendOffer,\n      emitDriverLocationUpdate,\n      // ðŸ†• YENÄ°: ÅžofÃ¶r konum gÃ¼ncelleme (RAM)\n      // ðŸ†• Daily.co Call Signaling\n      emitCallInvite,\n      emitCallAccepted,\n      emitCallRejected,\n      // ðŸ†• YENÄ°: Sync Call Events\n      emitCallAccept,\n      emitCallReject,\n      emitCallCancel,\n      emitCallEnd,\n      acceptDailyCall,\n      r