 _react.useRef)(new Animated.default.Value(1)).current;\n    const opacityAnim = (0, _react.useRef)(new Animated.default.Value(0.7)).current;\n    const animationRef = (0, _react.useRef)(null);\n    const opacityAnimRef = (0, _react.useRef)(null);\n    (0, _react.useEffect)(() => {\n      // Basit scale animasyonu\n      animationRef.current = Animated.default.loop(Animated.default.sequence([Animated.default.timing(scaleAnim, {\n        toValue: 1.15,\n        duration: 800,\n        useNativeDriver: true\n      }), Animated.default.timing(scaleAnim, {\n        toValue: 1,\n        duration: 800,\n        useNativeDriver: true\n      })]));\n      animationRef.current.start();\n\n      // Opacity animasyonu\n      opacityAnimRef.current = Animated.default.loop(Animated.default.sequence([Animated.default.timing(opacityAnim, {\n        toValue: 1,\n        duration: 800,\n        useNativeDriver: true\n      }), Animated.default.timing(opacityAnim, {\n        toValue: 0.7,\n        duration: 800,\n        useNativeDriver: true\n      })]));\n      opacityAnimRef.current.start();\n      return () => {\n        animationRef.current?.stop();\n        opacityAnimRef.current?.stop();\n      };\n    }, []);\n    const handlePress = () => {\n      Animated.default.sequence([Animated.default.timing(scaleAnim, {\n        toValue: 0.9,\n        duration: 100,\n        useNativeDriver: true\n      }), Animated.default.spring(scaleAnim, {\n        toValue: 1.15,\n        useNativeDriver: true\n      })]).start();\n      onPress();\n    };\n    return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(TouchableOpacity.default, {\n      onPress: handlePress,\n      disabled: loading,\n      activeOpacity: 0.8,\n      style: styles.callButtonContainer,\n      children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Animated.default.View, {\n        style: [{\n          transform: [{\n            scale: scaleAnim\n          }],\n          opacity: opacityAnim\n        }],\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_expoLinearGradient.LinearGradient, {\n          colors: [COLORS.primary, COLORS.secondary],\n          start: {\n            x: 0,\n            y: 0\n          },\n          end: {\n            x: 1,\n            y: 1\n          },\n          style: styles.gradientButton,\n          children: loading ? /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(ActivityIndicator.default, {\n            size: \"large\",\n            color: \"#FFF\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 990,\n            columnNumber: 13\n          }, this) : /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactJsxDevRuntime.Fragment, {\n            children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_expoVectorIcons.Ionicons, {\n              name: \"call\",\n              size: 60,\n              color: \"#FFF\"\n            }, void 0, false, {\n              fileName: _jsxFileName,\n              lineNumber: 993,\n              columnNumber: 15\n            }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n              style: styles.callButtonText,\n              children: \"\\xC7A\\u011ERI\"\n            }, void 0, false, {\n              fileName: _jsxFileName,\n              lineNumber: 994,\n              columnNumber: 15\n            }, this)]\n          }, void 0, true)\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 983,\n          columnNumber: 9\n        }, this)\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 982,\n        columnNumber: 7\n      }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Animated.default.View, {\n        style: [styles.pulseRing, {\n          transform: [{\n            scale: scaleAnim\n          }],\n          opacity: opacityAnim\n        }]\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 1001,\n        columnNumber: 7\n      },