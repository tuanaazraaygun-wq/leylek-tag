     if (Platform.default.OS !== 'android') {\n        console.log('ðŸ” iOS - izinler otomatik isteniyor');\n        setMicrophonePermission(true);\n        setCameraPermission(true);\n        setPermissionsGranted(true);\n        setPermissionChecking(false);\n        return true;\n      }\n      console.log('ðŸ” Android - TÃ¼m izinler isteniyor...');\n      try {\n        // Core permissions\n        const permissions = [_reactNativeWebDistIndex.PermissionsAndroid.PERMISSIONS.RECORD_AUDIO, _reactNativeWebDistIndex.PermissionsAndroid.PERMISSIONS.CAMERA];\n        console.log('ðŸ” Ä°stenen izinler:', permissions);\n        const results = await _reactNativeWebDistIndex.PermissionsAndroid.requestMultiple(permissions);\n        console.log('ðŸ” Ä°zin sonuÃ§larÄ±:', JSON.stringify(results, null, 2));\n        const audioGranted = results[_reactNativeWebDistIndex.PermissionsAndroid.PERMISSIONS.RECORD_AUDIO] === 'granted';\n        const cameraGranted = results[_reactNativeWebDistIndex.PermissionsAndroid.PERMISSIONS.CAMERA] === 'granted';\n        setMicrophonePermission(audioGranted);\n        setCameraPermission(cameraGranted);\n\n        // En Ã¶nemli izin: RECORD_AUDIO - bu olmadan arama yapÄ±lamaz\n        if (!audioGranted) {\n          console.log('âŒ RECORD_AUDIO izni REDDEDÄ°LDÄ° - Arama yapÄ±lamaz!');\n          Alert.default.alert('Mikrofon Ä°zni Gerekli', 'Sesli ve gÃ¶rÃ¼ntÃ¼lÃ¼ arama yapabilmek iÃ§in mikrofon izni vermeniz gerekiyor. LÃ¼tfen ayarlardan izin verin.', [{\n            text: 'Tamam',\n            onPress: () => Linking.default.openSettings()\n          }]);\n          setPermissionsGranted(false);\n          setPermissionChecking(false);\n          return false;\n        }\n        console.log('âœ… RECORD_AUDIO izni verildi');\n        console.log(cameraGranted ? 'âœ… CAMERA izni verildi' : 'âš ï¸ CAMERA izni reddedildi (opsiyonel)');\n        setPermissionsGranted(true);\n        setPermissionChecking(false);\n        return true;\n      } catc