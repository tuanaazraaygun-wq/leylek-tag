    const [callEnded, setCallEnded] = (0, _react.useState)(false);\n    const [receiverOffline, setReceiverOffline] = (0, _react.useState)(false);\n\n    // Arama kilidi\n    const isCallActiveRef = (0, _react.useRef)(false);\n\n    // ==================== SOCKET.IO HOOK - ÅžOFÃ–R ====================\n    const {\n      isConnected: socketConnected,\n      isRegistered: socketRegistered,\n      startCall: socketStartCall,\n      acceptCall: socketAcceptCall,\n      rejectCall: socketRejectCall,\n      endCall: socketEndCall,\n      // TAG & Teklif iÃ§in yeni fonksiyonlar\n      emitSendOffer: socketSendOffer,\n      // ðŸ†• Daily.co fonksiyonlarÄ±\n      acceptDailyCall,\n      rejectDailyCall,\n      endDailyCall\n    } = (0, useSocket.default)({\n      userId: user?.id || null,\n      userRole: 'driver',\n      // ðŸ†• Daily.co Gelen Arama - ÅžOFÃ–R\n      onIncomingDailyCall: data => {\n        console.log('ðŸ“¹ ÅžOFÃ–R - DAILY.CO GELEN ARAMA:', data);\n        setDailyRoomUrl(data.room_url);\n        setDailyRoomName(data.room_name);\n        setDailyCallType(data.call_type);\n        setDailyCallerId(data.caller_id);\n        setDailyCallerName('Yolcu');\n        setIncomingDailyCall(true);\n      },\n      onDailyCallAccepted: data => {\n        console.log('âœ… ÅžOFÃ–R - DAILY.CO ARAMA KABUL EDÄ°LDÄ°:', data);\n        setDailyCallActive(true);\n        setIncomingDailyCall(false);\n      },\n      onDailyCallRejected: data => {\n        console.log('âŒ ÅžOFÃ–R - DAILY.CO ARAMA REDDEDÄ°LDÄ°:', data);\n        setDailyCallActive(false);\n        setIncomingDailyCall(false);\n        setDailyRoomUrl(null);\n        Alert.default.alert('Bilgi', 'Arama reddedildi');\n      },\n      onDailyCallEnded: data => {\n        console.log('ðŸ“´ ÅžOFÃ–R - DAILY.CO ARAMA BÄ°TTÄ°:', data);\n        setDailyCallActive(false);\n        setIncomingDailyCall(false);\n        setDailyRoomUrl(null);\n      },\n      // Eski Agora eventleri - artÄ±k kullanÄ±lmÄ±yor\n      onIncom