');\n        return;\n      }\n      console.log('âœ… Hedef var, loading baÅŸlÄ±yor...');\n      setLoading(true);\n      try {\n        // GPS konumu varsa kullan, yoksa mock konum\n        const pickupLat = userLocation?.latitude || 41.0082;\n        const pickupLng = userLocation?.longitude || 28.9784;\n        const response = await fetch(`${API_URL}/passenger/create-request?user_id=${user.id}`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            pickup_location: 'Mevcut Konumunuz',\n            dropoff_location: destination.address,\n            pickup_lat: pickupLat,\n            pickup_lng: pickupLng,\n            dropoff_lat: destination.latitude,\n            dropoff_lng: destination.longitude,\n            notes: 'Hedef belirlendi'\n          })\n        });\n        const data = await response.json();\n        if (data.success) {\n          setActiveTag(data.tag);\n          Alert.default.alert('âœ… Ã‡aÄŸrÄ± GÃ¶nderildi', `YakÄ±ndaki sÃ¼rÃ¼cÃ¼ler \"${destination.address}\" iÃ§in tekliflerini gÃ¶nderiyor...`);\n        } else {\n          Alert.default.alert('Hata', data.detail || 'Ã‡aÄŸrÄ± gÃ¶nderilemedi');\n        }\n      } catch (error) {\n        Alert.default.alert('Hata', 'Ã‡aÄŸrÄ± gÃ¶nderilemedi');\n      } finally {\n        setLoading(false);\n      }\n    };\n\n    // SESLÄ° ARAMA - Mock fonksiyon\n    const handleVoiceCall = () => {\n      setCalling(true);\n      Alert.default.alert('ğŸ“ Sesli Arama', 'SÃ¼rÃ¼cÃ¼nÃ¼zle baÄŸlantÄ± kuruluyor...\\n\\nğŸ”’ UÃ§tan uca ÅŸifreli arama\\nğŸ“± GerÃ§ek numaralar gizli', [{\n        text: 'AramayÄ± SonlandÄ±r',\n        onPress: () => {\n          setCalling(false);\n          Alert.default.alert('ğŸ“ Arama SonlandÄ±rÄ±ldÄ±', 'Arama baÅŸarÄ±yla sonlandÄ±rÄ±ldÄ±.');\n        }\n      }]);\n    };\n    const handleAcceptOffer = async offerId => {\n      if (!activeTag) return;\n      Alert.default.al