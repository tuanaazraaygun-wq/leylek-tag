.default, {\n        visible: showVoiceCall,\n        remoteUserName: selectedDriverName,\n        channelName: activeTag.id,\n        userId: user.id,\n        isVideoCall: isVideoCall,\n        isCaller: isCallCaller,\n        onEnd: () => {\n          setShowVoiceCall(false);\n          setIsVideoCall(false);\n          setIsCallCaller(false);\n        },\n        onRejected: () => {\n          setShowVoiceCall(false);\n          setIsVideoCall(false);\n          setIsCallCaller(false);\n        }\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 2811,\n        columnNumber: 9\n      }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Modal.default, {\n        visible: showTripEndModal,\n        transparent: true,\n        animationType: \"fade\",\n        onRequestClose: () => setShowTripEndModal(false),\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n          style: styles.tripEndModalOverlay,\n          children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n            style: styles.tripEndModalContainer,\n            children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n              style: styles.tripEndModalHeader,\n              children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_expoVectorIcons.Ionicons, {\n                name: \"alert-circle\",\n                size: 50,\n                color: \"#3FA9F5\"\n              }, void 0, false, {\n                fileName: _jsxFileName,\n                lineNumber: 2841,\n                columnNumber: 15\n              }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n                style: styles.tripEndModalTitle,\n                children: \"Yolculuk Sonland\\u0131rma\"\n              }, void 0, false, {\n                fileName: _jsxFileName,\n                lineNumber: 2842,\n                columnNumber: 15\n              }, this)]\n            }, void 0, true, {\n        