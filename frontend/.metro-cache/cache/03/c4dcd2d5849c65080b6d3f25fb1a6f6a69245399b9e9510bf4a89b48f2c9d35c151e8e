ast g√∂ster\n          Alert.default.alert('Gizlendi', 'Bu talep 10 dakika boyunca g√∂r√ºnmeyecek');\n        }\n      } catch (error) {\n        console.log('Dismiss error:', error);\n      }\n    };\n    const submitOffer = async () => {\n      if (!offerPrice || isNaN(Number(offerPrice))) {\n        Alert.default.alert('Hata', 'Ge√ßerli bir fiyat girin');\n        return;\n      }\n\n      // √áift tƒ±klamayƒ± √∂nle\n      if (offerSending) return;\n      setOfferSending(true);\n      try {\n        console.log('üì§ Teklif g√∂nderiliyor...', selectedTagForOffer, offerPrice);\n\n        // Konum bilgisini ekle - OSRM mesafe hesaplamasƒ± i√ßin GEREKLƒ∞\n        const response = await fetch(`${API_URL}/driver/send-offer?user_id=${user.id}`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            tag_id: selectedTagForOffer,\n            price: Number(offerPrice),\n            estimated_time: 15,\n            notes: 'Hemen geliyorum!',\n            latitude: currentLocation?.latitude,\n            longitude: currentLocation?.longitude\n          })\n        });\n        const data = await response.json();\n        console.log('üì• Teklif yanƒ±tƒ±:', data);\n        if (data.success) {\n          // Buton ye≈üile d√∂ns√ºn\n          setOfferSent(true);\n          // Hemen kapat\n          setTimeout(() => {\n            setOfferModalVisible(false);\n            setOfferSent(false);\n            setOfferSending(false);\n            setOfferPrice('');\n            loadRequests();\n          }, 500);\n        } else {\n          setOfferSending(false);\n          Alert.default.alert('Hata', data.detail || 'Teklif g√∂nderilemedi');\n        }\n      } catch (error) {\n        console.log('‚ùå Teklif hatasƒ±:', error);\n        setOfferSending(false);\n        Alert.default.alert('Hata', 'Teklif g√∂nderilemedi');\n      }\n    };\n    const handleStartTag = async () =>