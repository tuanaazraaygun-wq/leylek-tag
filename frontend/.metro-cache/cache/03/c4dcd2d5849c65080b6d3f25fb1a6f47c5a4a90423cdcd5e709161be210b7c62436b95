'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, c => {\n        const r = Math.random() * 16 | 0;\n        const v = c === 'x' ? r : r & 0x3 | 0x8;\n        return v.toString(16);\n      });\n      const tempTagId = generateUUID();\n      const newRequestId = generateUUID(); // ðŸ†• Her TAG iÃ§in unique request_id\n\n      const tempTag = {\n        id: tempTagId,\n        user_id: user.id,\n        pickup_location: pickupAddress,\n        dropoff_location: destination.address,\n        pickup_lat: pickupLat,\n        pickup_lng: pickupLng,\n        dropoff_lat: destination.latitude,\n        dropoff_lng: destination.longitude,\n        status: 'pending',\n        created_at: new Date().toISOString()\n      };\n\n      // 1ï¸âƒ£ ANINDA UI'Ä± gÃ¼ncelle\n      setActiveTag(tempTag);\n      setCurrentRequestId(newRequestId); // ðŸ†• request_id kaydet\n      setLoading(false);\n\n      // 2ï¸âƒ£ ðŸ†• YENÄ°: Socket ile 20km iÃ§indeki ÅŸofÃ¶rlere yayÄ±nla\n      if (emitCreateTagRequest) {\n        emitCreateTagRequest({\n          request_id: newRequestId,\n          tag_id: tempTagId,\n          passenger_id: user.id,\n          passenger_name: user.name || user.phone,\n          pickup_location: pickupAddress,\n          pickup_lat: pickupLat,\n          pickup_lng: pickupLng,\n          dropoff_location: destination.address,\n          dropoff_lat: destination.latitude,\n          dropoff_lng: destination.longitude,\n          notes: 'Hedef belirlendi'\n        });\n        console.log('ðŸ”¥ TAG REQUEST Socket ile 20km ÅŸofÃ¶rlere yayÄ±nlandÄ±! request_id:', newRequestId);\n      } else if (emitNewTag) {\n        // Fallback: Eski yÃ¶ntem (tÃ¼m ÅŸofÃ¶rlere)\n        emitNewTag({\n          tag_id: tempTagId,\n          passenger_id: user.id,\n          passenger_name: user.name || user.phone,\n          pickup_lat: pickupLat,\n          pickup_lng: pickupLng,\n          pickup_address: pickupAddress,\n          dropoff_lat: destination.latitude,\n          dro