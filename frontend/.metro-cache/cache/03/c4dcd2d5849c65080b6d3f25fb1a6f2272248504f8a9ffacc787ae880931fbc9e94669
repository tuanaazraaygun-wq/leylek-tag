 return Math.round(distance * 10) / 10; // 1 ondalÄ±k basamak\n  };\n\n  // Leylek TAG Colors\n  const COLORS = {\n    primary: '#3FA9F5',\n    secondary: '#FF6B35',\n    background: '#FFFFFF',\n    surface: '#E9EDF2',\n    text: '#1B1B1E',\n    gray400: '#ADB5BD',\n    gray500: '#6C757D',\n    gray600: '#495057',\n    success: '#00C853',\n    info: '#007AFF'\n  };\n\n  // User Context\n\n  function App() {\n    _s2();\n    const [user, setUser] = (0, _react.useState)(null);\n    const [loading, setLoading] = (0, _react.useState)(true);\n    const [screen, setScreen] = (0, _react.useState)('login');\n\n    // Splash Screen\n    const [showSplash, setShowSplash] = (0, _react.useState)(true);\n\n    // KVKK OnayÄ±\n    const [kvkkAccepted, setKvkkAccepted] = (0, _react.useState)(false);\n    const [showKVKKModal, setShowKVKKModal] = (0, _react.useState)(false);\n    const [showSupportModal, setShowSupportModal] = (0, _react.useState)(false);\n\n    // Auth states\n    const [phone, setPhone] = (0, _react.useState)('');\n    const [otp, setOtp] = (0, _react.useState)('');\n    const [name, setName] = (0, _react.useState)('');\n    const [selectedCity, setSelectedCity] = (0, _react.useState)('');\n    const [cities, setCities] = (0, _react.useState)([]);\n    const [showCityPicker, setShowCityPicker] = (0, _react.useState)(false);\n\n    // Yeni Auth states\n    const [firstName, setFirstName] = (0, _react.useState)('');\n    const [lastName, setLastName] = (0, _react.useState)('');\n    const [pin, setPin] = (0, _react.useState)('');\n    const [confirmPin, setConfirmPin] = (0, _react.useState)('');\n    const [showPin, setShowPin] = (0, _react.useState)(false);\n    const [userExists, setUserExists] = (0, _react.useState)(false);\n    const [hasPin, setHasPin] = (0, _react.useState)(false);\n    const [deviceId, setDeviceId] = (0, _react.useState)(''); // Cihaz ID\n    const [isDeviceVerified, setIsDeviceVerified] = (0, _react.useState)(false); // Cihaz doÄŸrulanmÄ±ÅŸ