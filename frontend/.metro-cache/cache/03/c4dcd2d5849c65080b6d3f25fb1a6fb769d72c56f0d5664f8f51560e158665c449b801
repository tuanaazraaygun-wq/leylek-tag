aki\"\n            }, void 0, false, {\n              fileName: _jsxFileName,\n              lineNumber: 1700,\n              columnNumber: 17\n            }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_expoVectorIcons.Ionicons, {\n              name: \"chevron-down\",\n              size: 28,\n              color: \"#FFF\"\n            }, void 0, false, {\n              fileName: _jsxFileName,\n              lineNumber: 1701,\n              columnNumber: 17\n            }, this)]\n          }, void 0, true, {\n            fileName: _jsxFileName,\n            lineNumber: 1699,\n            columnNumber: 15\n          }, this)]\n        }, void 0, true, {\n          fileName: _jsxFileName,\n          lineNumber: 1691,\n          columnNumber: 11\n        }, this)]\n      }, void 0, true, {\n        fileName: _jsxFileName,\n        lineNumber: 1585,\n        columnNumber: 9\n      }, this)\n    }, void 0, false, {\n      fileName: _jsxFileName,\n      lineNumber: 1584,\n      columnNumber: 7\n    }, this);\n  }\n\n  // ==================== SIMPLE PULSE BUTTON ====================\n  _s5(FullScreenOfferCard, \"4xxi0o6goV+k7r85xfdhP/2sOFA=\");\n  _c6 = FullScreenOfferCard;\n  function AnimatedPulseButton({\n    onPress,\n    loading\n  }) {\n    _s6();\n    const scaleAnim = (0, _react.useRef)(new Animated.default.Value(1)).current;\n    const opacityAnim = (0, _react.useRef)(new Animated.default.Value(0.7)).current;\n    const animationRef = (0, _react.useRef)(null);\n    const opacityAnimRef = (0, _react.useRef)(null);\n    (0, _react.useEffect)(() => {\n      // Basit scale animasyonu\n      animationRef.current = Animated.default.loop(Animated.default.sequence([Animated.default.timing(scaleAnim, {\n        toValue: 1.15,\n        duration: 800,\n        useNativeDriver: true\n      }), Animated.default.timing(scaleAnim, {\n        toValue: 1,\n        duration: 800,\n        useNativeDriver: true\n      })]));\n      animationRef.current.start();\n\n      /