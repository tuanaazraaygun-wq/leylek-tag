          Alert.default.alert('Hata', data.detail || 'Hedef güncellenemedi');\n          }\n        } catch (error) {\n          Alert.default.alert('Hata', 'Hedef güncellenemedi');\n        }\n      }\n    };\n\n    // TEKLİFLER VARSA TAM EKRAN GÖS TER\n    // TikTok tarzı tam ekran teklif listesi - YOLCU\n    if (offers.length > 0 && activeTag && activeTag.status !== 'matched' && activeTag.status !== 'in_progress') {\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n        style: styles.tikTokContainer,\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(FlatList.default, {\n          data: offers,\n          keyExtractor: (item, index) => item.id || index.toString(),\n          renderItem: ({\n            item,\n            index\n          }) => /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(TikTokOfferCard, {\n            offer: item,\n            index: index,\n            total: offers.length,\n            onAccept: () => handleAcceptOffer(item.id),\n            isPassenger: true\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 2272,\n            columnNumber: 13\n          }, this),\n          pagingEnabled: true,\n          showsVerticalScrollIndicator: false,\n          snapToInterval: SCREEN_HEIGHT,\n          decelerationRate: \"fast\",\n          bounces: false,\n          onMomentumScrollEnd: e => {\n            const index = Math.round(e.nativeEvent.contentOffset.y / SCREEN_HEIGHT);\n            setCurrentOfferIndex(index);\n          }\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 2268,\n          columnNumber: 9\n        }, this)\n      }, void 0, false, {\n        fileName: _jsxFileName,\n        lineNumber: 2267,\n        columnNumber: 7\n      }, this);\n    }\n    return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n      style: styles.container,\n      children: [showToast && /*#__