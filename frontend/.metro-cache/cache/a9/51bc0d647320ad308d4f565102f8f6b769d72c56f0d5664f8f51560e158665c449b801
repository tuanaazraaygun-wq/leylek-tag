ır',\n        onPress: () => {\n          setCalling(false);\n          Alert.default.alert('📞 Arama Sonlandırıldı', 'Arama başarıyla sonlandırıldı.');\n        }\n      }]);\n    };\n    return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n      style: styles.container,\n      children: [!(activeTag && (activeTag.status === 'matched' || activeTag.status === 'in_progress')) && /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n        style: styles.modernHeader,\n        children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(TouchableOpacity.default, {\n          onPress: () => setScreen('role-select'),\n          style: styles.backButtonHeader,\n          children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_expoVectorIcons.Ionicons, {\n            name: \"chevron-back\",\n            size: 24,\n            color: \"#3FA9F5\"\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 3106,\n            columnNumber: 13\n          }, this)\n        }, void 0, false, {\n          fileName: _jsxFileName,\n          lineNumber: 3105,\n          columnNumber: 11\n        }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(View.default, {\n          style: styles.headerCenter,\n          children: [/*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n            style: styles.modernHeaderTitle,\n            children: user.name\n          }, void 0, false, {\n            fileName: _jsxFileName,\n            lineNumber: 3109,\n            columnNumber: 13\n          }, this), /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(Text.default, {\n            style: styles.modernHeaderSubtitle,\n            children: [\"\\u2B50 \", user.rating || '5.0']\n          }, void 0, true, {\n            fileName: _jsxFileName,\n            lineNumber: 3110,\n            columnNumber: 13\n          }, this)]\n        }, void 0, true, {\n          fileName: _jsxFileName,\n          lineNumbe