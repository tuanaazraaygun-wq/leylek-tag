 {\n        const location = await Location.getCurrentPositionAsync({\n          accuracy: Location.Accuracy.High\n        });\n        const coords = {\n          latitude: location.coords.latitude,\n          longitude: location.coords.longitude\n        };\n        setUserLocation(coords);\n\n        // Backend'e g√∂nder\n        await fetch(`${API_URL}/user/update-location?user_id=${user.id}&latitude=${coords.latitude}&longitude=${coords.longitude}`, {\n          method: 'POST'\n        });\n      } catch (error) {\n        console.error('Konum alƒ±namadƒ±:', error);\n      }\n    };\n    const handleRegister = async () => {\n      if (!name) {\n        Alert.default.alert('Hata', 'Adƒ±nƒ±zƒ± girin');\n        return;\n      }\n      if (!selectedCity) {\n        Alert.default.alert('Hata', '≈ûehir se√ßimi yapmalƒ±sƒ±nƒ±z');\n        return;\n      }\n      try {\n        const response = await fetch(`${API_URL}/auth/register`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            phone,\n            name,\n            city: selectedCity\n          })\n        });\n        const data = await response.json();\n        if (data.success) {\n          await saveUser(data.user);\n          Alert.default.alert('‚úÖ Kayƒ±t Ba≈üarƒ±lƒ±', 'Hesabƒ±nƒ±z olu≈üturuldu!');\n          setScreen('role-select'); // Kayƒ±ttan sonra rol se√ßimi\n        } else {\n          Alert.default.alert('Hata', data.detail || 'Kayƒ±t olu≈üturulamadƒ±');\n        }\n      } catch (error) {\n        Alert.default.alert('Hata', 'Kayƒ±t olu≈üturulamadƒ±');\n      }\n    };\n\n    // ==================== RENDER SCREENS ====================\n    if (loading) {\n      return /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(_reactNativeSafeAreaContext.SafeAreaView, {\n        style: styles.container,\n        children: /*#__PURE__*/(0, _reactJsxDevRuntime.jsxDEV)(ActivityIndicator.default, {\n      