sÃ¼resi (90 saniye countdown)\n    const [timeLeft, setTimeLeft] = (0, _react.useState)(90);\n\n    // Animasyonlar\n    const scaleAnim = (0, _react.useRef)(new Animated.default.Value(0.95)).current;\n    const fadeAnim = (0, _react.useRef)(new Animated.default.Value(0)).current;\n    const pulseAnim = (0, _react.useRef)(new Animated.default.Value(1)).current;\n\n    // GiriÅŸ animasyonu\n    (0, _react.useEffect)(() => {\n      Animated.default.parallel([Animated.default.spring(scaleAnim, {\n        toValue: 1,\n        tension: 60,\n        friction: 8,\n        useNativeDriver: true\n      }), Animated.default.timing(fadeAnim, {\n        toValue: 1,\n        duration: 250,\n        useNativeDriver: true\n      })]).start();\n    }, []);\n\n    // Countdown timer\n    (0, _react.useEffect)(() => {\n      if (!isPassenger) return; // Sadece yolcu tarafÄ±nda gÃ¶ster\n\n      const timer = setInterval(() => {\n        setTimeLeft(prev => {\n          if (prev <= 1) {\n            clearInterval(timer);\n            return 0;\n          }\n          return prev - 1;\n        });\n      }, 1000);\n      return () => clearInterval(timer);\n    }, [isPassenger]);\n\n    // Pulse animasyonu (yeni teklif iÃ§in)\n    (0, _react.useEffect)(() => {\n      if (index === 0 && isPassenger) {\n        Animated.default.loop(Animated.default.sequence([Animated.default.timing(pulseAnim, {\n          toValue: 1.02,\n          duration: 1000,\n          useNativeDriver: true\n        }), Animated.default.timing(pulseAnim, {\n          toValue: 1,\n          duration: 1000,\n          useNativeDriver: true\n        })])).start();\n      }\n    }, [index, isPassenger]);\n\n    // Hesaplamalar\n    const arrivalTime = driverArrivalMin || offer.estimated_arrival_min || Math.round((offer.distance_to_passenger_km || 5) / 40 * 60);\n    const distanceToPassengerKm = offer.distance_to_passenger_km?.toFixed(1) || offer.distance_km?.toFixed(1) || '?';\n    const tripDistanceKm = offer.trip_dist