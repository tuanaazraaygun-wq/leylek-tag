erLocation, setDriverLocation] = (0, _react.useState)(null);\n\n    // Toast notification state\n    const [showToast, setShowToast] = (0, _react.useState)(false);\n    const [toastMessage, setToastMessage] = (0, _react.useState)('');\n\n    // Mesafe ve s√ºre state'leri\n    const [realDistance, setRealDistance] = (0, _react.useState)(0);\n    const [estimatedTime, setEstimatedTime] = (0, _react.useState)(0);\n\n    // Arama state'leri (sesli ve g√∂r√ºnt√ºl√º)\n    const [showVoiceCall, setShowVoiceCall] = (0, _react.useState)(false);\n    const [isVideoCall, setIsVideoCall] = (0, _react.useState)(false);\n    const [selectedDriverName, setSelectedDriverName] = (0, _react.useState)('');\n    const [isCallCaller, setIsCallCaller] = (0, _react.useState)(false); // BEN Mƒ∞ ARIYORUM?\n\n    // Gelen arama state'leri\n    const [showIncomingCall, setShowIncomingCall] = (0, _react.useState)(false);\n    const [incomingCallInfo, setIncomingCallInfo] = (0, _react.useState)(null);\n\n    // Kar≈üƒ±lƒ±klƒ± iptal sistemi state'leri\n    const [showTripEndModal, setShowTripEndModal] = (0, _react.useState)(false);\n    const [tripEndRequesterType, setTripEndRequesterType] = (0, _react.useState)(null);\n\n    // Refs for polling (closure problem fix) - YOLCU\n    const passengerCallStateRef = (0, _react.useRef)({\n      showVoiceCall: false,\n      showIncomingCall: false,\n      isCallCaller: false\n    });\n    (0, _react.useEffect)(() => {\n      passengerCallStateRef.current = {\n        showVoiceCall,\n        showIncomingCall,\n        isCallCaller\n      };\n    }, [showVoiceCall, showIncomingCall, isCallCaller]);\n\n    // Gelen arama polling - YOLCU i√ßin\n    (0, _react.useEffect)(() => {\n      // Ba≈ülangƒ±√ß kontrol√º\n      if (!user?.id || !activeTag) return;\n      if (activeTag.status !== 'matched' && activeTag.status !== 'in_progress') return;\n      let isActive = true;\n      const checkIncomingCall = async () => {\n        // G√ºncel state'leri ref'ten oku (closure fix)\n        const {\n          showVoiceCall: inCall,\n          showIncomingCall: hasIncoming,\n          isCallCaller: isCaller\n        } = passengerCallStateRef.current;\n\n        // Aramadaysa veya ben arƒ±yorsam polling yapma\n        if (!isActive || inCall || isCaller) return;\n        try {\n          const response = await fetch(`${API_URL}/voice/check-incoming?user_id=${user.id}`);\n          if (!isActive) return;\n          if (!response.ok) return;\n          const text = await response.text();\n          if (!text || text.trim() === '') return;\n          const data = JSON.parse(text);\n\n          // ARAYAN KAPATTI MI KONTROL√ú - IncomingCall a√ßƒ±kken\n          if (hasIncoming && data.success && data.call_cancelled) {\n            console.log('üìû YOLCU - Arayan aramayƒ± kapattƒ±, modal kapatƒ±lƒ±yor');\n            setShowIncomingCall(false);\n            setIncomingCallInfo(null);\n            return;\n          }\n\n          // Gelen arama yoksa ve modal a√ßƒ±ksa kapat (arayan vazge√ßti)\n          if (hasIncoming && data.success && !data.has_incoming && !data.call_cancelled) {\n            console.log('üìû YOLCU - Arama artƒ±k yok, modal kapatƒ±lƒ±yor');\n            setShowIncomingCall(false);\n            setIncomingCallInfo(null);\n            return;\n          }\n\n          // Son kontrol - ref'ten g√ºncel deƒüerleri al\n          const currentState = passengerCallStateRef.current;\n          if (!isActive || currentState.showVoiceCall || currentState.showIncomingCall) return;\n          if (data.success && data.has_incoming && data.call) {\n            console.log('üìû YOLCU - GELEN ARAMA!', data.call.caller_name);\n            setIncomingCallInfo({\n              callerName: data.call.caller_name,\n              callType: data.call.call_type || 'audio',\n              channelName: data.call.channel_name\n            });\n            setShowIncomingCall(true);\n          }\n        } catch (error) {\n          // Sessiz kal\n        }\n      };\n\n      // ƒ∞lk kontrol√º hemen yap\n      checkIncomingCall();\n      // Sonra her 1.5 saniyede bir kontrol et (daha hƒ±zlƒ± senkronizasyon)\n      const interval = setInterval(checkIncomingCall, 1500);\n      return () => {\n        isActive = false;\n        clearInterval(interval);\n      };\n    }, [user?.id, activeTag?.id, activeTag?.status]);\n\n    // Ara butonu animasyonu\n    const buttonPulse = (0, _react.useRef)(new Animated.default.Value(1)).current;\n    const destinationButtonScale = (0, _react.useRef)(new Animated.default.Value(1)).current;\n    const arrowPosition = (0, _react.useRef)(new Animated.default.Value(-100)).current;\n    (0, _react.useEffect)(() => {\n      // Ara butonu pulse\n      Animated.default.loop(Animated.default.sequence([Animated.default.timing(buttonPulse, {\n        toValue: 1.1,\n        duration: 1000,\n        useNativeDriver: true\n      }), Animated.default.timing(buttonPulse, {\n        toValue: 1,\n        duration: 1000,\n        useNativeDriver: true\n      })])).start();\n\n      // Hedef se√ßin butonu pulse\n      Animated.default.loop(Animated.default.sequence([Animated.default.timing(destinationButtonScale, {\n        toValue: 1.05,\n        duration: 1500,\n        useNativeDriver: true\n      }), Animated.default.timing(destinationButtonScale, {\n        toValue: 1,\n        duration: 1500,\n        useNativeDriver: true\n      })])).start();\n    }, []);\n\n    // CANLI KONUM G√úNCELLEME - E≈üle≈üince ba≈üla\n    (0, _react.useEffect)(() => {\n      if (activeTag && (activeTag.status === 'matched' || activeTag.status === 'in_progress')) {\n        const interval = setInterval(async () => {\n          try {\n            // S√ºr√ºc√º konumunu backend'den al\n            const response = await fetch(`${API_URL}/passenger/driver-location/${activeTag.driver_id}`);\n            const data = await response.json();\n            if (data.location) {\n              setDriverLocation(data.location);\n              // Mesafeyi hesapla\n              if (userLocation) {\n                const distance = calculateDistance(userLocation.latitude, userLocation.longitude, data.location.latitude, data.location.longitude);\n                setRealDistance(distance);\n                // Tahmini s√ºreyi hesapla (ortalama 40 km/h)\n                const time = Math.round(distance / 40 * 60);\n                setEstimatedTime(time);\n              }\n            }\n          } catch (error) {\n            console.log('Konum alƒ±namadƒ±:', error);\n          }\n        }, 5000); // 5 saniyede bir g√ºncelle\n\n        return () => clearInterval(interval);\n      }\n    }, [activeTag, userLocation]);\n\n    // GELEN ARAMA KONTROL√ú - Polling (Yolcu i√ßin)\n    (0, _react.useEffect)(() => {\n      if (activeTag && (activeTag.status === 'matched' || activeTag.status === 'in_progress') && !showVoiceCall && !showIncomingCall) {\n        const checkIncoming = async () => {\n          try {\n            const response = await fetch(`${API_URL}/voice/check-incoming?user_id=${user.id}`);\n            if (!response.ok) return;\n            const text = await response.text();\n            if (!text || text.trim() === '') return;\n            const data = JSON.parse(text);\n            if (data.success && data.has_incoming && data.call) {\n              console.log('üìû YOLCU - GELEN ARAMA!', data.call.caller_name);\n              setIncomingCallInfo({\n                callerName: data.call.caller_name,\n                callType: data.call.call_type || 'audio',\n                channelName: data.call.channel_name\n              });\n              setShowIncomingCall(true);\n            }\n          } catch (error) {\n            if (!(error instanceof SyntaxError)) {\n              console.log('Gelen arama kontrol√º hatasƒ±:', error);\n            }\n          }\n        };\n\n        // ƒ∞lk kontrol√º hemen yap\n        checkIncoming();\n        // Sonra her 2 saniyede bir kontrol e