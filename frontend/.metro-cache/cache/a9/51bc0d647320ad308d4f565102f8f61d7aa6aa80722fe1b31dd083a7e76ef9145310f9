     setPermissionsGranted(true);\n        setPermissionChecking(false);\n        return true;\n      } catch (error) {\n        console.error('ðŸ” Ä°zin hatasÄ±:', error);\n        setPermissionChecking(false);\n        return false;\n      }\n    };\n\n    // Uygulama baÅŸlangÄ±cÄ±nda izinleri iste - HEMEN\n    (0, _react.useEffect)(() => {\n      const checkPermissions = async () => {\n        console.log('ðŸ” Uygulama baÅŸladÄ± - Ä°zin kontrolÃ¼ baÅŸlÄ±yor...');\n        await requestAllPermissions();\n      };\n\n      // Splash screen beklemeden hemen izin iste\n      checkPermissions();\n    }, []);\n\n    // Device ID oluÅŸtur veya al\n    const getOrCreateDeviceId = async () => {\n      try {\n        let storedDeviceId = await AsyncStorage.default.getItem('device_id');\n        if (!storedDeviceId) {\n          // Yeni cihaz ID oluÅŸtur\n          storedDeviceId = 'device_' + Date.now() + '_' + Math.random().toString(36).substring(2, 15);\n          await AsyncStorage.default.setItem('device_id', storedDeviceId);\n          console.log('ðŸ†” Yeni cihaz ID oluÅŸturuldu:', storedDeviceId);\n        } else {\n          console.log('ðŸ†” Mevcut cihaz ID:', storedDeviceId);\n        }\n        return storedDeviceId;\n      } catch (error) {\n        console.error('Device ID hatasÄ±:', error);\n        return 'device_' + Date.now();\n      }\n    };\n    (0, _react.useEffect)(() => {\n      initializeApp();\n    }, []);\n    const initializeApp = async () => {\n      // Ã–nce cihaz ID'yi al\n      const dId = await getOrCreateDeviceId();\n      setDeviceId(dId);\n\n      // KVKK onayÄ± kontrolÃ¼ (telefon numarasÄ±na gÃ¶re)\n      const storedKVKKPhone = await AsyncStorage.default.getItem('kvkk_accepted_phone');\n      // Onay varsa set et\n      if (storedKVKKPhone) {\n        setKvkkAccepted(true);\n      }\n\n      // Sonra kullanÄ±cÄ±yÄ± yÃ¼kle\n      await loadUser();\n    };\n\n    // KVKK onayÄ±nÄ± kaydet (telefon numarasÄ±na gÃ¶re)\n    const saveKVKKCons