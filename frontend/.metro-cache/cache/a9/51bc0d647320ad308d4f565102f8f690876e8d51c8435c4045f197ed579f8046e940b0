      },\n          body: JSON.stringify({\n            phone,\n            device_id: currentDeviceId\n          })\n        });\n        const checkData = await checkResponse.json();\n        console.log('ðŸ” Check user response:', checkData);\n        if (checkData.success && checkData.user_exists) {\n          // KullanÄ±cÄ± kayÄ±tlÄ±\n          setHasPin(checkData.has_pin || false);\n          setUserExists(true);\n          setIsDeviceVerified(checkData.device_verified || false);\n          if (checkData.has_pin && checkData.device_verified) {\n            // PIN var VE cihaz doÄŸrulanmÄ±ÅŸ - direkt PIN ekranÄ±na git\n            Alert.default.alert('HoÅŸ Geldiniz! ðŸ‘‹', `${checkData.user_name || 'KullanÄ±cÄ±'}, 6 haneli ÅŸifrenizi girin`);\n            setScreen('enter-pin');\n          } else if (checkData.has_pin && !checkData.device_verified) {\n            // PIN var AMA yeni cihaz - OTP doÄŸrula sonra PIN iste\n            const response = await fetch(`${API_URL}/auth/send-otp`, {\n              method: 'POST',\n              headers: {\n                'Content-Type': 'application/json'\n              },\n              body: JSON.stringify({\n                phone\n              })\n            });\n            const data = await response.json();\n            if (data.success) {\n              Alert.default.alert('Yeni Cihaz DoÄŸrulamasÄ± ðŸ“±', 'Bu cihazdan ilk giriÅŸ. SMS ile gÃ¶nderilen kodu girin.\\n\\nTest: 123456');\n              setScreen('otp');\n            }\n          } else {\n            // PIN yok - OTP doÄŸrula ve PIN oluÅŸtur\n            const response = await fetch(`${API_URL}/auth/send-otp`, {\n              method: 'POST',\n              headers: {\n                'Content-Type': 'application/json'\n              },\n              body: JSON.stringify({\n                phone\n              })\n            });\n            const data = await response.json();\n            if (data.success) {\n              Alert.default.alert('Åžif