) => {\n      if (!user) return;\n      try {\n        const location = await Location.getCurrentPositionAsync({\n          accuracy: Location.Accuracy.High\n        });\n        const coords = {\n          latitude: location.coords.latitude,\n          longitude: location.coords.longitude\n        };\n        setUserLocation(coords);\n\n        // Backend'e gÃ¶nder\n        await fetch(`${API_URL}/user/update-location?user_id=${user.id}&latitude=${coords.latitude}&longitude=${coords.longitude}`, {\n          method: 'POST'\n        });\n      } catch (error) {\n        console.error('Konum alÄ±namadÄ±:', error);\n      }\n    };\n    const handleRegister = async () => {\n      if (!name) {\n        Alert.default.alert('Hata', 'AdÄ±nÄ±zÄ± girin');\n        return;\n      }\n      if (!selectedCity) {\n        Alert.default.alert('Hata', 'Åžehir seÃ§imi yapmalÄ±sÄ±nÄ±z');\n        return;\n      }\n      try {\n        const response = await fetch(`${API_URL}/auth/register`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            phone,\n            name,\n            city: selectedCity\n          })\n        });\n        const data = await response.json();\n        if (data.success) {\n          await saveUser(data.user);\n          setScreen('role-select'); // KayÄ±ttan sonra rol seÃ§imi\n        } else {\n          Alert.default.alert('Hata', data.detail || 'KayÄ±t oluÅŸturulamadÄ±');\n        }\n      } catch (error) {\n        Alert.default.alert('Hata', 'KayÄ±t oluÅŸturulamadÄ±');\n      }\n    };\n\n    // ==================== RENDER SCREENS ====================\n\n    // SPLASH SCREEN - 3 saniye gÃ¶ster - daha gÃ¼venilir timeout ile\n    (0, _react.useEffect)(() => {\n      if (showSplash) {\n        const splashTimer = setTimeout(() => {\n          console.log('ðŸŽ¬ Splash timeout - login ekranÄ±na geÃ§iliyor');\n          setShowSplash(false);\n          